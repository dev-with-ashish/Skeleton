    </div>
  </section>

  <section class="ai-seo-section-{{ai_gen_id}}">
    <h2>{{ block.settings.silo1_h2 }}</h2>
    <div class="ai-seo-grid-{{ai_gen_id}}">
      <div class="ai-seo-card-{{ai_gen_id}}">
        <h3>{{ block.settings.product1_title }}</h3>
        <p>{{ block.settings.product1_desc }}</p>
        <a href="{{ block.settings.product1_url }}">{{ block.settings.product1_link_text }}</a>
      </div>
      <div class="ai-seo-card-{{ai_gen_id}}">
        <h3>{{ block.settings.product2_title }}</h3>
        <p>{{ block.settings.product2_desc }}</p>
        <a href="{{ block.settings.product2_url }}">{{ block.settings.product2_link_text }}</a>
      </div>
      <div class="ai-seo-card-{{ai_gen_id}}">
        <h3>{{ block.settings.product3_title }}</h3>
        <p>{{ block.settings.product3_desc }}</p>
        <a href="{{ block.settings.product3_url }}">{{ block.settings.product3_link_text }}</a>
      </div>
    </div>
  </section>

  <section class="ai-seo-section-{{ai_gen_id}}">
    <h2>{{ block.settings.trending_h2 }}</h2>
    <p style="text-align: center;">{{ block.settings.trending_desc }}</p>
  </section>

  <section class="ai-seo-section-{{ai_gen_id}}">
    <h2>{{ block.settings.silo2_h2 }}</h2>
    <div class="ai-seo-grid-{{ai_gen_id}}">
      <div class="ai-seo-card-{{ai_gen_id}}">
        <h3>{{ block.settings.service1_title }}</h3>
        <p>{{ block.settings.service1_desc }}</p>
        <a href="{{ block.settings.service1_url }}">{{ block.settings.service1_link_text }}</a>
      </div>
      <div class="ai-seo-card-{{ai_gen_id}}">
        <h3>{{ block.settings.service2_title }}</h3>
        <p>{{ block.settings.service2_desc }}</p>
        <a href="{{ block.settings.service2_url }}">{{ block.settings.service2_link_text }}</a>
      </div>
      <div class="ai-seo-card-{{ai_gen_id}}">
        <h3>{{ block.settings.service3_title }}</h3>
        <p>{{ block.settings.service3_desc }}</p>
        <a href="{{ block.settings.service3_url }}">{{ block.settings.service3_link_text }}</a>
      </div>
    </div>
    <div style="text-align: center;">
