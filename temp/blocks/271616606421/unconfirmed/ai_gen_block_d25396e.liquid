      <h2>{{ block.settings.reviews_h2 }}</h2>
      <p>{{ block.settings.reviews_text }}</p>
    </div>
  </section>

  <section class="ai-seo-section-{{ai_gen_id}}">
    <div class="ai-seo-contact-{{ai_gen_id}}">
      <h2>{{ block.settings.contact_h2 }}</h2>
      <div class="ai-seo-contact-info-{{ai_gen_id}}">
        <p><strong>Address:</strong> {{ block.settings.address }}</p>
        <p><strong>Phone:</strong> {{ block.settings.phone }}</p>
        <p><strong>Email:</strong> {{ block.settings.email }}</p>
        <p><strong>Hours:</strong> {{ block.settings.hours }}</p>
      </div>
    </div>
