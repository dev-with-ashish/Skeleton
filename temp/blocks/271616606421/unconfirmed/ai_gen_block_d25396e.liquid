      </a>
    </div>
  </section>

  <section class="ai-seo-section-{{ai_gen_id}}">
    <h2>{{ block.settings.usps_h2 }}</h2>
    <div class="ai-seo-usps-{{ai_gen_id}}">
      <div class="ai-seo-usp-{{ai_gen_id}}">
        <h4>{{ block.settings.usp1_title }}</h4>
        <p>{{ block.settings.usp1_desc }}</p>
      </div>
      <div class="ai-seo-usp-{{ai_gen_id}}">
        <h4>{{ block.settings.usp2_title }}</h4>
        <p>{{ block.settings.usp2_desc }}</p>
      </div>
      <div class="ai-seo-usp-{{ai_gen_id}}">
        <h4>{{ block.settings.usp3_title }}</h4>
        <p>{{ block.settings.usp3_desc }}</p>
      </div>
      <div class="ai-seo-usp-{{ai_gen_id}}">
        <h4>{{ block.settings.usp4_title }}</h4>
        <p>{{ block.settings.usp4_desc }}</p>
