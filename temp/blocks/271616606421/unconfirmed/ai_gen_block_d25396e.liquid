</div>

<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@graph": [
    {
      "@type": "Organization",
      "name": "{{ block.settings.business_name }}",
      "url": "{{ shop.url }}",
      "logo": "{{ block.settings.logo_url }}",
      "description": "{{ block.settings.business_description }}",
      "contactPoint": {
        "@type": "ContactPoint",
        "telephone": "{{ block.settings.phone }}",
        "contactType": "Customer Service",
        "areaServed": "IN",
        "availableLanguage": ["English", "Hindi"]
      },
      "sameAs": [
        "{{ block.settings.facebook_url }}",
        "{{ block.settings.instagram_url }}",
        "{{ block.settings.linkedin_url }}"
      ]
    },
    {
      "@type": "LocalBusiness",
      "name": "{{ block.settings.business_name }}",
      "image": "{{ block.settings.logo_url }}",
      "address": {
        "@type": "PostalAddress",
        "streetAddress": "{{ block.settings.street_address }}",
        "addressLocality": "{{ block.settings.city }}",
        "addressRegion": "{{ block.settings.state }}",
        "postalCode": "{{ block.settings.postal_code }}",
        "addressCountry": "IN"
      },
      "geo": {
        "@type": "GeoCoordinates",
        "latitude": "{{ block.settings.latitude }}",
        "longitude": "{{ block.settings.longitude }}"
      },
      "url": "{{ shop.url }}",
      "telephone": "{{ block.settings.phone }}",
      "email": "{{ block.settings.email }}",
      "openingHoursSpecification": {
        "@type": "OpeningHoursSpecification",
        "dayOfWeek": ["Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"],
        "opens": "{{ block.settings.opening_time }}",
        "closes": "{{ block.settings.closing_time }}"
      },
      "priceRange": "{{ block.settings.price_range }}",
      "aggregateRating": {
        "@type": "AggregateRating",
        "ratingValue": "{{ block.settings.rating_value }}",
        "reviewCount": "{{ block.settings.review_count }}"
      }
    },
    {
      "@type": "WebPage",
      "name": "{{ block.settings.h1_title }}",
      "description": "{{ block.settings.hero_usp }}",
      "url": "{{ shop.url }}",
      "mainEntity": {
        "@type": "ItemList",
        "itemListElement": [
          {
            "@type": "ListItem",
            "position": 1,
            "name": "{{ block.settings.product1_title }}",
            "url": "{{ block.settings.product1_url }}"
          },
          {
            "@type": "ListItem",
            "position": 2,
            "name": "{{ block.settings.product2_title }}",
            "url": "{{ block.settings.product2_url }}"
          },
          {
            "@type": "ListItem",
            "position": 3,
            "name": "{{ block.settings.product3_title }}",
            "url": "{{ block.settings.product3_url }}"
          }
        ]
      }
    },
    {
      "@type": "Service",
      "serviceType": "UV Printing Services",
      "provider": {
        "@type": "LocalBusiness",
        "name": "{{ block.settings.business_name }}"
      },
      "areaServed": {
        "@type": "City",
        "name": "Noida"
      },
      "hasOfferCatalog": {
        "@type": "OfferCatalog",
        "name": "Printing Services",
        "itemListElement": [
          {
            "@type": "Offer",
            "itemOffered": {
              "@type": "Service",
              "name": "{{ block.settings.service1_title }}",
              "description": "{{ block.settings.service1_desc }}"
            }
          },
          {
            "@type": "Offer",
            "itemOffered": {
              "@type": "Service",
              "name": "{{ block.settings.service2_title }}",
              "description": "{{ block.settings.service2_desc }}"
            }
          },
          {
            "@type": "Offer",
            "itemOffered": {
              "@type": "Service",
              "name": "{{ block.settings.service3_title }}",
              "description": "{{ block.settings.service3_desc }}"
            }
          }
        ]
      }
    },
    {
      "@type": "BreadcrumbList",
      "itemListElement": [
        {
          "@type": "ListItem",
          "position": 1,
          "name": "Home",
          "item": "{{ shop.url }}"
        },
        {
          "@type": "ListItem",
          "position": 2,
          "name": "{{ block.settings.silo1_h2 }}",
          "item": "{{ block.settings.product1_url }}"
        },
        {
          "@type": "ListItem",
          "position": 3,
          "name": "{{ block.settings.silo2_h2 }}",
          "item": "{{ block.settings.service1_url }}"
        }
      ]
    }
  ]
}
</script>

