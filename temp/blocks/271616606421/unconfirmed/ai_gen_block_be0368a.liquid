  <section class="ai-seo-homepage__section-{{ ai_gen_id }}">
    <h2 class="ai-seo-homepage__h2-{{ ai_gen_id }}">{{ block.settings.reviews_h2 }}</h2>
    <div class="ai-seo-homepage__reviews-{{ ai_gen_id }}">
      {% if block.settings.review1_text != blank %}
