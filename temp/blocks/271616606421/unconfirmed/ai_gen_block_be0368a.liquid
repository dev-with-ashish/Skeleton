  </section>

  <section class="ai-seo-homepage__section-{{ ai_gen_id }}">
    <h2 class="ai-seo-homepage__h2-{{ ai_gen_id }}">{{ block.settings.contact_h2 }}</h2>
    <div class="ai-seo-homepage__contact-{{ ai_gen_id }}">
      <div class="ai-seo-homepage__contact-info-{{ ai_gen_id }}">
        {% if block.settings.contact_address != blank %}
          <div>
            <strong>Address:</strong>
            {{ block.settings.contact_address }}
          </div>
        {% endif %}
        {% if block.settings.contact_phone != blank %}
