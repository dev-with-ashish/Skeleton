{% doc %}
  @prompt
    Create an SEO-optimized homepage layout with a clear silo structure:
    
    H1: Premium Wall Art & Professional UV Printing Services
    
    Hero: USP + two CTAs (Shop Wall Art / Printing Services)
    
    Silo 1 (H2): Shop Premium Wall Art Collections
    
    Metal Posters (link)
    
    Canvas Prints (link)
    
    Acrylic Posters (link)
    
    Short SEO intro under each
    
    H2: Trending & Best Selling Wall Art (product grid/slider)
    
    Silo 2 (H2): Professional Printing Services in Noida
    
    UV Printing (link)
    
    Eco Solvent (link)
    
    Flex Printing (link)
    
    Button: Custom Job Enquiry
    
    H2: Why Choose Aarohi PixelPrint (USPs)
    
    H2: Loved by 1000+ Customers (reviews/social proof)
    
    H2: Visit Our Printing Studio in Noida (address + map + contact), add google schema json-ld data to tell everything we have on this page, add hero bg 5 changing bgs changing in background, make it dark themed and option to add multiple categories and services also schema in loop also add customer ratings with profile and ratings and texts, when hovered the blocks should glow also give me option to add category and services images in these blocks
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-seo-dark-{{ai_gen_id}} {
    width: 100%;
    background-color: {{ block.settings.bg_color }};
    color: {{ block.settings.text_color }};
  }

  .ai-seo-dark-{{ai_gen_id}} * {
    box-sizing: border-box;
  }

  .ai-seo-hero-{{ai_gen_id}} {
    position: relative;
    color: {{ block.settings.hero_text_color }};
    padding: 80px 20px;
    text-align: center;
    overflow: hidden;
    min-height: 500px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-seo-hero-bg-{{ai_gen_id}} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 0;
  }

  .ai-seo-hero-bg-image-{{ai_gen_id}} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
    opacity: 0;
    transition: opacity 1.5s ease-in-out;
  }

  .ai-seo-hero-bg-image-{{ai_gen_id}}.active {
    opacity: 1;
  }

  .ai-seo-hero-overlay-{{ai_gen_id}} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: {{ block.settings.hero_overlay_color }};
    opacity: {{ block.settings.hero_overlay_opacity }};
    z-index: 1;
  }

  .ai-seo-hero-content-{{ai_gen_id}} {
    position: relative;
    z-index: 2;
    max-width: 1200px;
    width: 100%;
  }

  .ai-seo-hero-{{ai_gen_id}} h1 {
    font-size: 48px;
    margin: 0 0 20px;
    line-height: 1.2;
  }

  .ai-seo-hero-{{ai_gen_id}} p {
    font-size: 20px;
    margin: 0 0 30px;
    max-width: 700px;
    margin-left: auto;
    margin-right: auto;
  }

  .ai-seo-hero-ctas-{{ai_gen_id}} {
    display: flex;
    gap: 15px;
    justify-content: center;
    flex-wrap: wrap;
  }

  .ai-seo-cta-{{ai_gen_id}} {
    display: inline-block;
    padding: 15px 30px;
    text-decoration: none;
    border-radius: 5px;
    font-size: 16px;
    font-weight: 600;
    transition: all 0.3s ease;
  }

  .ai-seo-cta-primary-{{ai_gen_id}} {
    background-color: {{ block.settings.cta_primary_bg }};
    color: {{ block.settings.cta_primary_text }};
  }

  .ai-seo-cta-primary-{{ai_gen_id}}:hover {
    background-color: {{ block.settings.cta_primary_hover }};
  }

  .ai-seo-cta-secondary-{{ai_gen_id}} {
    background-color: {{ block.settings.cta_secondary_bg }};
    color: {{ block.settings.cta_secondary_text }};
  }

  .ai-seo-cta-secondary-{{ai_gen_id}}:hover {
    background-color: {{ block.settings.cta_secondary_hover }};
  }

  .ai-seo-section-{{ai_gen_id}} {
    padding: 60px 20px;
    max-width: 1200px;
    margin: 0 auto;
  }

  .ai-seo-section-{{ai_gen_id}} h2 {
    font-size: 36px;
    margin: 0 0 30px;
    text-align: center;
    color: {{ block.settings.heading_color }};
  }

  .ai-seo-grid-{{ai_gen_id}} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
    gap: 30px;
    margin-bottom: 40px;
  }

  .ai-seo-card-{{ai_gen_id}} {
    background-color: {{ block.settings.card_bg_color }};
    padding: 30px;
    border-radius: 8px;
    border: 1px solid {{ block.settings.card_border_color }};
    transition: all 0.3s ease;
    position: relative;
    overflow: hidden;
  }

  .ai-seo-card-{{ai_gen_id}}:hover {
    transform: translateY(-5px);
    border-color: {{ block.settings.card_hover_border }};
    box-shadow: 0 0 20px {{ block.settings.glow_color }}, 0 0 40px {{ block.settings.glow_color }};
  }

  .ai-seo-card-image-{{ai_gen_id}} {
    width: 100%;
    height: 200px;
    object-fit: cover;
    border-radius: 6px;
    margin-bottom: 20px;
  }

  .ai-seo-card-image-placeholder-{{ai_gen_id}} {
    width: 100%;
    height: 200px;
    background-color: #3a3a3a;
    border-radius: 6px;
    margin-bottom: 20px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-seo-card-image-placeholder-{{ai_gen_id}} svg {
    width: 80px;
    height: 80px;
    opacity: 0.3;
  }

  .ai-seo-card-{{ai_gen_id}} h3 {
    font-size: 24px;
    margin: 0 0 15px;
    color: {{ block.settings.card_heading_color }};
  }

  .ai-seo-card-{{ai_gen_id}} p {
    font-size: 16px;
    line-height: 1.6;
    margin: 0 0 15px;
    color: {{ block.settings.card_text_color }};
  }

  .ai-seo-card-{{ai_gen_id}} a {
    color: {{ block.settings.link_color }};
    text-decoration: none;
    font-weight: 600;
  }

  .ai-seo-card-{{ai_gen_id}} a:hover {
    text-decoration: underline;
  }

  .ai-seo-usps-{{ai_gen_id}} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 20px;
  }

  .ai-seo-usp-{{ai_gen_id}} {
    text-align: center;
    padding: 20px;
  }

  .ai-seo-usp-{{ai_gen_id}} h4 {
    font-size: 20px;
    margin: 0 0 10px;
    color: {{ block.settings.heading_color }};
  }

  .ai-seo-reviews-{{ai_gen_id}} {
    background-color: {{ block.settings.reviews_bg_color }};
    padding: 40px;
    border-radius: 8px;
    border: 1px solid {{ block.settings.card_border_color }};
  }

  .ai-seo-reviews-grid-{{ai_gen_id}} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 20px;
    margin-top: 30px;
  }

  .ai-seo-review-card-{{ai_gen_id}} {
    background-color: {{ block.settings.card_bg_color }};
    padding: 20px;
    border-radius: 8px;
    border: 1px solid {{ block.settings.card_border_color }};
    transition: all 0.3s ease;
  }

  .ai-seo-review-card-{{ai_gen_id}}:hover {
    transform: translateY(-3px);
    box-shadow: 0 0 15px {{ block.settings.glow_color }}, 0 0 30px {{ block.settings.glow_color }};
  }

  .ai-seo-review-header-{{ai_gen_id}} {
    display: flex;
    align-items: center;
    gap: 15px;
    margin-bottom: 15px;
  }

  .ai-seo-review-avatar-{{ai_gen_id}} {
    width: 50px;
    height: 50px;
    border-radius: 50%;
    background-color: {{ block.settings.avatar_bg_color }};
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 20px;
    font-weight: 600;
    color: {{ block.settings.avatar_text_color }};
    flex-shrink: 0;
  }

  .ai-seo-review-info-{{ai_gen_id}} {
    flex-grow: 1;
  }

  .ai-seo-review-name-{{ai_gen_id}} {
    font-weight: 600;
    margin: 0 0 5px;
    color: {{ block.settings.card_heading_color }};
  }

  .ai-seo-review-stars-{{ai_gen_id}} {
    display: flex;
    gap: 2px;
    color: {{ block.settings.star_color }};
  }

  .ai-seo-review-text-{{ai_gen_id}} {
    font-size: 14px;
    line-height: 1.6;
    color: {{ block.settings.card_text_color }};
  }

  .ai-seo-contact-{{ai_gen_id}} {
    background-color: {{ block.settings.contact_bg_color }};
    padding: 40px;
    border-radius: 8px;
    border: 1px solid {{ block.settings.card_border_color }};
  }

  .ai-seo-contact-info-{{ai_gen_id}} {
    margin-bottom: 20px;
  }

  .ai-seo-contact-info-{{ai_gen_id}} p {
    margin: 10px 0;
    font-size: 16px;
  }

  .ai-seo-enquiry-btn-{{ai_gen_id}} {
    display: inline-block;
    padding: 15px 30px;
    background-color: {{ block.settings.enquiry_btn_bg }};
    color: {{ block.settings.enquiry_btn_text_color }};
    text-decoration: none;
    border-radius: 5px;
    font-weight: 600;
    margin-top: 20px;
    transition: all 0.3s ease;
  }

  .ai-seo-enquiry-btn-{{ai_gen_id}}:hover {
    background-color: {{ block.settings.enquiry_btn_hover }};
    box-shadow: 0 0 20px {{ block.settings.glow_color }};
  }

  @media (max-width: 768px) {
    .ai-seo-hero-{{ai_gen_id}} {
      min-height: 400px;
      padding: 60px 20px;
    }

    .ai-seo-hero-{{ai_gen_id}} h1 {
      font-size: 32px;
    }

    .ai-seo-hero-{{ai_gen_id}} p {
      font-size: 18px;
    }

    .ai-seo-section-{{ai_gen_id}} h2 {
      font-size: 28px;
    }

    .ai-seo-grid-{{ai_gen_id}} {
      grid-template-columns: 1fr;
    }

    .ai-seo-reviews-grid-{{ai_gen_id}} {
      grid-template-columns: 1fr;
    }
  }
{% endstyle %}

<div class="ai-seo-dark-{{ai_gen_id}}" {{ block.shopify_attributes }}>
  <section class="ai-seo-hero-{{ai_gen_id}}">
    <div class="ai-seo-hero-bg-{{ai_gen_id}}">
      {% if block.settings.hero_bg_1 %}
        <img
          src="{{ block.settings.hero_bg_1 | image_url: width: 2000 }}"
          alt="Hero background 1"
          class="ai-seo-hero-bg-image-{{ai_gen_id}} active"
          loading="eager"
          width="{{ block.settings.hero_bg_1.width }}"
          height="{{ block.settings.hero_bg_1.height }}"
        >
      {% endif %}
      {% if block.settings.hero_bg_2 %}
        <img
          src="{{ block.settings.hero_bg_2 | image_url: width: 2000 }}"
          alt="Hero background 2"
          class="ai-seo-hero-bg-image-{{ai_gen_id}}"
          loading="lazy"
          width="{{ block.settings.hero_bg_2.width }}"
          height="{{ block.settings.hero_bg_2.height }}"
        >
      {% endif %}
      {% if block.settings.hero_bg_3 %}
        <img
          src="{{ block.settings.hero_bg_3 | image_url: width: 2000 }}"
          alt="Hero background 3"
          class="ai-seo-hero-bg-image-{{ai_gen_id}}"
          loading="lazy"
          width="{{ block.settings.hero_bg_3.width }}"
          height="{{ block.settings.hero_bg_3.height }}"
        >
      {% endif %}
      {% if block.settings.hero_bg_4 %}
        <img
          src="{{ block.settings.hero_bg_4 | image_url: width: 2000 }}"
          alt="Hero background 4"
          class="ai-seo-hero-bg-image-{{ai_gen_id}}"
          loading="lazy"
          width="{{ block.settings.hero_bg_4.width }}"
          height="{{ block.settings.hero_bg_4.height }}"
        >
      {% endif %}
      {% if block.settings.hero_bg_5 %}
        <img
          src="{{ block.settings.hero_bg_5 | image_url: width: 2000 }}"
          alt="Hero background 5"
          class="ai-seo-hero-bg-image-{{ai_gen_id}}"
          loading="lazy"
          width="{{ block.settings.hero_bg_5.width }}"
          height="{{ block.settings.hero_bg_5.height }}"
        >
      {% endif %}
    </div>
    <div class="ai-seo-hero-overlay-{{ai_gen_id}}"></div>
    <div class="ai-seo-hero-content-{{ai_gen_id}}">
      <h1>{{ block.settings.h1_title }}</h1>
      <p>{{ block.settings.hero_usp }}</p>
      <div class="ai-seo-hero-ctas-{{ai_gen_id}}">
        <a href="{{ block.settings.cta1_url }}" class="ai-seo-cta-{{ai_gen_id}} ai-seo-cta-primary-{{ai_gen_id}}">
          {{ block.settings.cta1_text }}
        </a>
        <a href="{{ block.settings.cta2_url }}" class="ai-seo-cta-{{ai_gen_id}} ai-seo-cta-secondary-{{ai_gen_id}}">
          {{ block.settings.cta2_text }}
        </a>
      </div>
    </div>
  </section>

  {% if block.settings.show_categories %}
    <section class="ai-seo-section-{{ai_gen_id}}">
      <h2>{{ block.settings.categories_h2 }}</h2>
      <div class="ai-seo-grid-{{ai_gen_id}}">
        {% for i in (1..6) %}
          {% liquid
            assign title_key = 'category_' | append: i | append: '_title'
            assign desc_key = 'category_' | append: i | append: '_desc'
            assign url_key = 'category_' | append: i | append: '_url'
            assign link_key = 'category_' | append: i | append: '_link_text'
            assign image_key = 'category_' | append: i | append: '_image'
            
            assign title = block.settings[title_key]
            assign desc = block.settings[desc_key]
            assign url = block.settings[url_key]
            assign link_text = block.settings[link_key]
            assign image = block.settings[image_key]
          %}
          {% if title != blank %}
            <div class="ai-seo-card-{{ai_gen_id}}">
              {% if image %}
                <img
                  src="{{ image | image_url: width: 600 }}"
                  alt="{{ title }}"
                  class="ai-seo-card-image-{{ai_gen_id}}"
                  loading="lazy"
                  width="600"
                  height="400"
                >
              {% else %}
                <div class="ai-seo-card-image-placeholder-{{ai_gen_id}}">
                  {{ 'image' | placeholder_svg_tag }}
                </div>
              {% endif %}
              <h3>{{ title }}</h3>
              <p>{{ desc }}</p>
              {% if url != blank %}
                <a href="{{ url }}">{{ link_text }}</a>
              {% endif %}
            </div>
          {% endif %}
        {% endfor %}
      </div>
    </section>
  {% endif %}

  <section class="ai-seo-section-{{ai_gen_id}}">
    <h2>{{ block.settings.trending_h2 }}</h2>
    <p style="text-align: center;">{{ block.settings.trending_desc }}</p>
  </section>

  {% if block.settings.show_services %}
    <section class="ai-seo-section-{{ai_gen_id}}">
      <h2>{{ block.settings.services_h2 }}</h2>
      <div class="ai-seo-grid-{{ai_gen_id}}">
        {% for i in (1..6) %}
          {% liquid
            assign title_key = 'service_' | append: i | append: '_title'
            assign desc_key = 'service_' | append: i | append: '_desc'
            assign url_key = 'service_' | append: i | append: '_url'
            assign link_key = 'service_' | append: i | append: '_link_text'
            assign image_key = 'service_' | append: i | append: '_image'
            
            assign title = block.settings[title_key]
            assign desc = block.settings[desc_key]
            assign url = block.settings[url_key]
            assign link_text = block.settings[link_key]
            assign image = block.settings[image_key]
          %}
          {% if title != blank %}
            <div class="ai-seo-card-{{ai_gen_id}}">
              {% if image %}
                <img
                  src="{{ image | image_url: width: 600 }}"
                  alt="{{ title }}"
                  class="ai-seo-card-image-{{ai_gen_id}}"
                  loading="lazy"
                  width="600"
                  height="400"
                >
              {% else %}
                <div class="ai-seo-card-image-placeholder-{{ai_gen_id}}">
                  {{ 'image' | placeholder_svg_tag }}
                </div>
              {% endif %}
              <h3>{{ title }}</h3>
              <p>{{ desc }}</p>
              {% if url != blank %}
                <a href="{{ url }}">{{ link_text }}</a>
              {% endif %}
            </div>
          {% endif %}
        {% endfor %}
      </div>
      <div style="text-align: center;">
        <a href="{{ block.settings.enquiry_url }}" class="ai-seo-enquiry-btn-{{ai_gen_id}}">
          {{ block.settings.enquiry_button_label }}
        </a>
      </div>
    </section>
  {% endif %}

  <section class="ai-seo-section-{{ai_gen_id}}">
    <h2>{{ block.settings.usps_h2 }}</h2>
    <div class="ai-seo-usps-{{ai_gen_id}}">
      {% for i in (1..4) %}
        {% liquid
          assign title_key = 'usp_' | append: i | append: '_title'
          assign desc_key = 'usp_' | append: i | append: '_desc'
          
          assign title = block.settings[title_key]
          assign desc = block.settings[desc_key]
        %}
        {% if title != blank %}
          <div class="ai-seo-usp-{{ai_gen_id}}">
            <h4>{{ title }}</h4>
            <p>{{ desc }}</p>
          </div>
        {% endif %}
      {% endfor %}
    </div>
  </section>

  <section class="ai-seo-section-{{ai_gen_id}}">
    <div class="ai-seo-reviews-{{ai_gen_id}}">
      <h2>{{ block.settings.reviews_h2 }}</h2>
      <p style="text-align: center;">{{ block.settings.reviews_text }}</p>
      <div class="ai-seo-reviews-grid-{{ai_gen_id}}">
        {% for i in (1..6) %}
          {% liquid
            assign name_key = 'review_' | append: i | append: '_name'
            assign rating_key = 'review_' | append: i | append: '_rating'
            assign text_key = 'review_' | append: i | append: '_text'
            
            assign name = block.settings[name_key]
            assign rating = block.settings[rating_key]
            assign text = block.settings[text_key]
          %}
          {% if name != blank %}
            <div class="ai-seo-review-card-{{ai_gen_id}}">
              <div class="ai-seo-review-header-{{ai_gen_id}}">
                <div class="ai-seo-review-avatar-{{ai_gen_id}}">
                  {{ name | slice: 0 }}
                </div>
                <div class="ai-seo-review-info-{{ai_gen_id}}">
                  <div class="ai-seo-review-name-{{ai_gen_id}}">{{ name }}</div>
                  <div class="ai-seo-review-stars-{{ai_gen_id}}">
                    {% for star in (1..5) %}
                      {% if star <= rating %}
                        ★
                      {% else %}
                        ☆
                      {% endif %}
                    {% endfor %}
                  </div>
                </div>
              </div>
              <div class="ai-seo-review-text-{{ai_gen_id}}">{{ text }}</div>
            </div>
          {% endif %}
        {% endfor %}
      </div>
    </div>
  </section>

  <section class="ai-seo-section-{{ai_gen_id}}">
    <div class="ai-seo-contact-{{ai_gen_id}}">
      <h2>{{ block.settings.contact_h2 }}</h2>
      <div class="ai-seo-contact-info-{{ai_gen_id}}">
        <p><strong>Address:</strong> {{ block.settings.address }}</p>
        <p><strong>Phone:</strong> {{ block.settings.phone }}</p>
        <p><strong>Email:</strong> {{ block.settings.email }}</p>
        <p><strong>Hours:</strong> {{ block.settings.hours }}</p>
      </div>
    </div>
  </section>
</div>

<script>
  (function() {
    class HeroBackgroundSlider{{ai_gen_id}} extends HTMLElement {
      constructor() {
        super();
        this.images = this.querySelectorAll('.ai-seo-hero-bg-image-{{ai_gen_id}}');
        this.currentIndex = 0;
        this.interval = {{ block.settings.slide_interval | times: 1000 }};
      }

      connectedCallback() {
        if (this.images.length > 1) {
          this.startSlideshow();
        }
      }

      startSlideshow() {
        setInterval(() => {
          this.images[this.currentIndex].classList.remove('active');
          this.currentIndex = (this.currentIndex + 1) % this.images.length;
          this.images[this.currentIndex].classList.add('active');
        }, this.interval);
      }

      disconnectedCallback() {
        if (this.intervalId) {
          clearInterval(this.intervalId);
        }
      }
    }

    customElements.define('hero-background-slider-{{ai_gen_id}}', HeroBackgroundSlider{{ai_gen_id}});

    const heroSection = document.querySelector('.ai-seo-hero-{{ai_gen_id}}');
    if (heroSection) {
      const wrapper = document.createElement('hero-background-slider-{{ai_gen_id}}');
      const bgContainer = heroSection.querySelector('.ai-seo-hero-bg-{{ai_gen_id}}');
      if (bgContainer) {
        bgContainer.parentNode.insertBefore(wrapper, bgContainer);
        wrapper.appendChild(bgContainer);
      }
    }
  })();
</script>

