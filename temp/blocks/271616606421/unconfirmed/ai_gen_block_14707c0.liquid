      },
      "priceRange": "{{ block.settings.price_range }}",
      "aggregateRating": {
        "@type": "AggregateRating",
        "ratingValue": "{{ block.settings.avg_rating }}",
        "reviewCount": "{{ block.settings.total_reviews }}"
      }
    },
    {
      "@type": "WebPage",
      "name": "{{ block.settings.h1_title }}",
      "description": "{{ block.settings.hero_usp }}",
      "url": "{{ shop.url }}",
      "mainEntity": {
        "@type": "ItemList",
        "itemListElement": [
          {% for i in (1..6) %}
            {% liquid
              assign title_key = 'category_' | append: i | append: '_title'
              assign url_key = 'category_' | append: i | append: '_url'
              assign title = block.settings[title_key]
              assign url = block.settings[url_key]
            %}
            {% if title != blank %}
              {
                "@type": "ListItem",
                "position": {{ i }},
                "name": "{{ title }}",
                "url": "{{ url }}"
              }{% unless forloop.last %},{% endunless %}
            {% endif %}
          {% endfor %}
        ]
      }
    },
    {
      "@type": "Service",
      "serviceType": "Printing Services",
      "provider": {
        "@type": "LocalBusiness",
        "name": "{{ block.settings.business_name }}"
      },
      "areaServed": {
        "@type": "City",
        "name": "{{ block.settings.city }}"
      },
      "hasOfferCatalog": {
        "@type": "OfferCatalog",
        "name": "Printing Services",
        "itemListElement": [
          {% for i in (1..6) %}
            {% liquid
              assign title_key = 'service_' | append: i | append: '_title'
              assign desc_key = 'service_' | append: i | append: '_desc'
              assign title = block.settings[title_key]
              assign desc = block.settings[desc_key]
            %}
            {% if title != blank %}
              {
                "@type": "Offer",
                "itemOffered": {
                  "@type": "Service",
                  "name": "{{ title }}",
                  "description": "{{ desc }}"
                }
              }{% unless forloop.last %},{% endunless %}
            {% endif %}
          {% endfor %}
        ]
      }
    },
    {
      "@type": "BreadcrumbList",
      "itemListElement": [
        {
          "@type": "ListItem",
          "position": 1,
          "name": "Home",
          "item": "{{ shop.url }}"
        }
        {% for i in (1..6) %}
          {% liquid
            assign title_key = 'category_' | append: i | append: '_title'
            assign url_key = 'category_' | append: i | append: '_url'
            assign title = block.settings[title_key]
            assign url = block.settings[url_key]
          %}
          {% if title != blank and url != blank %}
            ,{
              "@type": "ListItem",
              "position": {{ i | plus: 1 }},
              "name": "{{ title }}",
              "item": "{{ url }}"
            }
          {% endif %}
        {% endfor %}
      ]
    }
  ]
}
</script>

