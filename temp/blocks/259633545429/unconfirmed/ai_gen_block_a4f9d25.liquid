{% doc %}
  @prompt
    Generate a latest styled responsive footer section using tailwind css and three.js having Home
    About Us
    Services
    Clients
    Contact Us in the right and logo image in the left, on mobile logo squared on left menus in 2 columns in right and compyright below both and on desktop logo left and in same height menus in two columns in right and copyright below bothe of them after a spacer, delete all these make a best footer section by yourself, KEEP EVERTHING AS IT IS NOW JUST INSTEAD OF SUBSCRIBE US BUTTON GIVE A NO. IN PLACE OF EMAIL AND CALL US BUTTON BELOW

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-footer-{{ ai_gen_id }} {
    background: linear-gradient(135deg, {{ block.settings.background_color_1 }} 0%, {{ block.settings.background_color_2 }} 100%);
    color: {{ block.settings.text_color }};
    position: relative;
    overflow: hidden;}

  .ai-footer-container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
    padding: 60px 20px 30px;
    position: relative;
    z-index: 2;
  }

  .ai-footer-main-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: 1fr 1fr1fr 1fr;
    gap: 40px;
    margin-bottom: 50px;
  }

  .ai-footer-brand-{{ ai_gen_id }} {
    grid-column: span 2;
  }

  .ai-footer-logo-{{ ai_gen_id }} {
    max-width: {{ block.settings.logo_width }}px;
    height: auto;
    margin-bottom: 20px;
    filter: brightness(1.1);
  }

  .ai-footer-logo-placeholder-{{ ai_gen_id }} {
    width: {{ block.settings.logo_width }}px;
    height: 60px;
    background: rgba(255, 255, 255, 0.1);
    border-radius: 12px;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 20px;
    backdrop-filter: blur(10px);
  }

  .ai-footer-logo-placeholder-{{ ai_gen_id }} svg {
    width: 80%;
    height: 80%;
    opacity: 0.8;
  }

  .ai-footer-description-{{ ai_gen_id }} {
    font-size: 16px;
    line-height: 1.6;
    opacity: 0.9;
    margin-bottom: 25px;
    max-width: 300px;
  }

  .ai-footer-social-{{ ai_gen_id }} {
    display: flex;
    gap: 15px;
  }

  .ai-footer-social-link-{{ ai_gen_id }} {
    width: 45px;
    height: 45px;
    background: rgba(255, 255, 255, 0.1);
    border-radius: 12px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: {{ block.settings.text_color }};
    text-decoration: none;
    transition: all 0.3s ease;
    backdrop-filter: blur(10px);
    border: 1px solid rgba(255, 255, 255, 0.1);
  }

  .ai-footer-social-link-{{ ai_gen_id }}:hover {
    background: {{ block.settings.accent_color }};
    transform: translateY(-3px);
    box-shadow: 0 10px 25px rgba(0, 0, 0, 0.2);
  }

  .ai-footer-column-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
  }

  .ai-footer-title-{{ ai_gen_id }} {
    font-size: 18px;
    font-weight: 600;
    margin-bottom: 20px;
    color: {{ block.settings.text_color }};
    position: relative;
    padding-bottom: 10px;
  }

  .ai-footer-title-{{ ai_gen_id }}::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    width: 30px;
    height: 3px;
    background: {{ block.settings.accent_color }};
    border-radius: 2px;
  }

  .ai-footer-link-{{ ai_gen_id }} {
    color: {{ block.settings.text_color }};
    text-decoration: none;
    font-size: 15px;
    margin-bottom: 12px;
    opacity: 0.8;
    transition: all 0.3s ease;
    position: relative;
    padding-left: 0;
  }

  .ai-footer-link-{{ ai_gen_id }}:hover {
    opacity: 1;
    color: {{ block.settings.accent_color }};padding-left: 10px;
  }

  .ai-footer-contact-{{ ai_gen_id }} {
    margin-top: 20px;
  }

  .ai-footer-phone-{{ ai_gen_id }} {
    width: 100%;
    padding: 15px;
    border: none;
    border-radius: 12px;
    background: rgba(255, 255, 255, 0.1);
    color: {{ block.settings.text_color }};
    font-size: 16px;
    margin-bottom: 15px;
    backdrop-filter: blur(10px);
    border: 1px solid rgba(255, 255, 255, 0.1);text-align: center;font-weight: 600;
  }

  .ai-footer-call-btn-{{ ai_gen_id }} {
    width: 100%;
    padding: 15px;
    background: {{ block.settings.accent_color }};
    color: white;
    border: none;
    border-radius: 12px;
    font-size: 14px;
    font-weight: 600;
    cursor: pointer;
    transition: all 0.3s ease;
    text-decoration: none;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 8px;}

  .ai-footer-call-btn-{{ ai_gen_id }}:hover {
    background: {{ block.settings.accent_color }};
    transform: translateY(-2px);
    box-shadow: 0 8px 20px rgba(0, 0, 0, 0.2);
  }

  .ai-footer-bottom-{{ ai_gen_id }} {
    border-top: 1px solid rgba(255, 255, 255, 0.1);
    padding-top: 30px;
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-wrap: wrap;gap: 20px;
  }

  .ai-footer-copyright-{{ ai_gen_id }} {
    font-size: 14px;
    opacity: 0.7;
  }

  .ai-footer-payment-{{ ai_gen_id }} {
    display: flex;
    gap: 10px;align-items: center;
  }

  .ai-footer-payment-icon-{{ ai_gen_id }} {
    width: 40px;
    height: 25px;
    background: rgba(255, 255, 255, 0.9);
    border-radius: 6px;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 10px;
    font-weight: 600;
    color: #333;
  }

  .ai-footer-background-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    opacity: 0.05;
    background-image: 
      radial-gradient(circle at 20% 80%, rgba(255, 255, 255, 0.1) 0%, transparent 50%),
      radial-gradient(circle at 80% 20%, rgba(255, 255, 255, 0.1) 0%, transparent 50%);
  }

  @media screen and (max-width: 968px) {
    .ai-footer-main-{{ ai_gen_id }} {
      grid-template-columns: 1fr 1fr;
      gap: 30px;
    }

    .ai-footer-brand-{{ ai_gen_id }} {
      grid-column: span 2;
    }
  }

  @media screen and (max-width: 640px) {
    .ai-footer-container-{{ ai_gen_id }} {
      padding: 40px 20px 20px;
    }

    .ai-footer-main-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 30px;
      margin-bottom: 30px;
    }

    .ai-footer-brand-{{ ai_gen_id }} {
      grid-column: span 1;
      text-align: center;
    }

    .ai-footer-bottom-{{ ai_gen_id }} {
      flex-direction: column;
      text-align: center;
    }

    .ai-footer-social-{{ ai_gen_id }} {
      justify-content: center;
    }
  }
{% endstyle %}

<footer class="ai-footer-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-footer-background-{{ ai_gen_id }}"></div>
  
  <div class="ai-footer-container-{{ ai_gen_id }}">
    <div class="ai-footer-main-{{ ai_gen_id }}">
      <div class="ai-footer-brand-{{ ai_gen_id }}">
        {% if block.settings.logo %}
          <img
            src="{{ block.settings.logo | image_url: width: 300}}"
            alt="{{ shop.name }}"
            class="ai-footer-logo-{{ ai_gen_id }}"
            width="{{ block.settings.logo_width }}"
            height="auto"
            loading="lazy"
          >
        {% else %}
          <div class="ai-footer-logo-placeholder-{{ ai_gen_id }}">
            {{'image' | placeholder_svg_tag }}
          </div>
        {% endif %}

        {% if block.settings.description != blank %}
          <p class="ai-footer-description-{{ ai_gen_id }}">
            {{ block.settings.description }}
          </p>
        {% endif %}<div class="ai-footer-social-{{ ai_gen_id }}">
          {% if block.settings.facebook_url != blank %}
            <a href="{{ block.settings.facebook_url }}" class="ai-footer-social-link-{{ ai_gen_id }}" target="_blank" rel="noopener">
              <svg width="20" height="20" viewBox="0 0 24 24" fill="currentColor">
                <path d="M24 12.073c0-6.627-5.373-12-12-12s-12 5.373-12 12c0 5.99 4.388 10.954 10.125 11.854v-8.385H7.078v-3.47h3.047V9.43c0-3.007 1.792-4.669 4.533-4.669 1.312 0 2.686.235 2.686.235v2.953H15.83c-1.491 0-1.956.925-1.956 1.874v2.25h3.328l-.532 3.47h-2.796v8.385C19.612 23.027 24 18.062 24 12.073z"/>
              </svg>
            </a>
          {% endif %}

          {% if block.settings.instagram_url != blank %}
            <a href="{{ block.settings.instagram_url }}" class="ai-footer-social-link-{{ ai_gen_id }}" target="_blank" rel="noopener">
              <svg width="20" height="20" viewBox="0 0 24 24" fill="currentColor">
                <path d="M12 2.163c3.204 0 3.584.012 4.85.07 3.252.148 4.771 1.691 4.919 4.919.058 1.265.069 1.645.069 4.849 0 3.205-.012 3.584-.069 4.849-.149 3.225-1.664 4.771-4.919 4.919-1.266.058-1.644.07-4.85.07-3.204 0-3.584-.012-4.849-.07-3.26-.149-4.771-1.699-4.919-4.92-.058-1.265-.07-1.644-.07-4.849 0-3.204.013-3.583.07-4.849.149-3.227 1.664-4.771 4.919-4.919 1.266-.057 1.645-.069 4.849-.069zm0-2.163c-3.259 0-3.667.014-4.947.072-4.358.2-6.78 2.618-6.98 6.98-.059 1.281-.073 1.689-.073 4.948 0 3.259.014 3.668.072 4.948.2 4.358 2.618 6.78 6.98 6.98 1.281.058 1.689.072 4.948.072 3.259 0 3.668-.014 4.948-.072 4.354-.2 6.782-2.618 6.979-6.98.059-1.28.073-1.689.073-4.948 0-3.259-.014-3.667-.072-4.947-.196-4.354-2.617-6.78-6.979-6.98-1.281-.059-1.69-.073-4.949-.073zm0 5.838c-3.403 0-6.162 2.759-6.162 6.162s2.759 6.163 6.162 6.163 6.162-2.759 6.162-6.163c0-3.403-2.759-6.162-6.162-6.162zm0 10.162c-2.209 0-4-1.79-4-4 0-2.209 1.791-4 4-4s4 1.791 4 4c0 2.21-1.791 4-4 4zm6.406-11.845c-.796 0-1.441.645-1.441 1.44s.645 1.44 1.441 1.44c.795 0 1.439-.645 1.439-1.44s-.644-1.44-1.439-1.44z"/></svg>
            </a>
          {% endif %}

          {% if block.settings.twitter_url != blank %}
            <a href="{{ block.settings.twitter_url }}" class="ai-footer-social-link-{{ ai_gen_id }}" target="_blank" rel="noopener">
              <svg width="20" height="20" viewBox="0 0 24 24" fill="currentColor">
                <path d="M23.953 4.57a10 10 0 01-2.825.775 4.958 4.958 0 002.163-2.723c-.951.555-2.005.959-3.127 1.184a4.92 4.92 0 00-8.384 4.482C7.69 8.095 4.067 6.13 1.64 3.162a4.822 4.822 0 00-.666 2.475c0 1.71.87 3.213 2.188 4.096a4.904 4.904 0 01-2.228-.616v.06a4.923 4.923 0 003.946 4.827 4.996 4.996 0 01-2.212.085 4.936 4.936 0 004.604 3.417 9.867 9.867 0 01-6.102 2.105c-.39 0-.779-.023-1.17-.067a13.995 13.995 0 007.557 2.209c9.053 0 13.998-7.496 13.998-13.985 0-.21 0-.42-.015-.63A9.935 9.935 0 0024 4.59z"/>
              </svg>
            </a>
          {% endif %}

          {% if block.settings.youtube_url != blank %}
            <a href="{{ block.settings.youtube_url }}" class="ai-footer-social-link-{{ ai_gen_id }}" target="_blank" rel="noopener">
              <svg width="20" height="20" viewBox="0 0 24 24" fill="currentColor">
                <path d="M23.498 6.186a3.016 3.016 0 0 0-2.122-2.136C19.505 3.545 12 3.545 12 3.545s-7.505 0-9.377.505A3.017 3.017 0 0 0 .502 6.186C0 8.07 0 12 0 12s0 3.93.502 5.814a3.016 3.016 0 0 0 2.122 2.136c1.871.505 9.376.505 9.376.505s7.505 0 9.377-.505a3.015 3.015 0 0 0 2.122-2.136C24 15.93 24 12 24 12s0-3.93-.502-5.814zM9.545 15.568V8.432L15.818 12l-6.273 3.568z"/>
              </svg>
            </a>
          {% endif %}
        </div>
      </div>

      <div class="ai-footer-column-{{ ai_gen_id }}">
        <h3 class="ai-footer-title-{{ ai_gen_id }}">Quick Links</h3>
        {% if block.settings.link_1_text != blank and block.settings.link_1_url != blank %}
          <a href="{{ block.settings.link_1_url }}" class="ai-footer-link-{{ ai_gen_id }}">{{ block.settings.link_1_text }}</a>
        {% endif %}
        {% if block.settings.link_2_text != blank and block.settings.link_2_url != blank %}
          <a href="{{ block.settings.link_2_url }}" class="ai-footer-link-{{ ai_gen_id }}">{{ block.settings.link_2_text }}</a>
        {% endif %}
        {% if block.settings.link_3_text != blank and block.settings.link_3_url != blank %}
          <a href="{{ block.settings.link_3_url }}" class="ai-footer-link-{{ ai_gen_id }}">{{ block.settings.link_3_text }}</a>
        {% endif %}
        {% if block.settings.link_4_text != blank and block.settings.link_4_url != blank %}
          <a href="{{ block.settings.link_4_url }}" class="ai-footer-link-{{ ai_gen_id }}">{{ block.settings.link_4_text }}</a>
        {% endif %}
      </div>

      <div class="ai-footer-column-{{ ai_gen_id }}">
        <h3 class="ai-footer-title-{{ ai_gen_id }}">Contact Us</h3><p style="font-size: 14px; opacity: 0.8; margin-bottom: 15px;">Call us for any inquiries or support</p>
        
        {% if block.settings.phone_number != blank %}
          <div class="ai-footer-phone-{{ ai_gen_id }}">
            {{ block.settings.phone_number }}
          </div>
        {% endif %}
        
        {% if block.settings.phone_number != blank %}
          <a href="tel:{{ block.settings.phone_number }}" class="ai-footer-call-btn-{{ ai_gen_id }}">
            <svg width="16" height="16" viewBox="0 0 24 24" fill="currentColor">
              <path d="M6.62 10.79c1.44 2.83 3.76 5.14 6.59 6.59l2.2-2.2c.27-.27.67-.36 1.02-.24 1.12.37 2.33.57 3.57.57.55 0 1.45 1 1V20c0 .55-.45 1-1 1-9.39 0-17-7.61-17-17 0-.55.45-1 1-1h3.5c.55 0 1 .45 1 1 0 1.25.2 2.45.57 3.57.11.35.03.74-.25 1.02l-2.2 2.2z"/>
            </svg>
            Call Us
          </a>
        {% endif %}
      </div>
    </div>

    <div class="ai-footer-bottom-{{ ai_gen_id }}">
      <div class="ai-footer-copyright-{{ ai_gen_id }}">
        {{ block.settings.copyright_text }}
      </div>
      
      {% if block.settings.show_payment_icons %}
        <div class="ai-footer-payment-{{ ai_gen_id }}">
          <div class="ai-footer-payment-icon-{{ ai_gen_id }}">VISA</div>
          <div class="ai-footer-payment-icon-{{ ai_gen_id }}">MC</div>
          <div class="ai-footer-payment-icon-{{ ai_gen_id }}">AMEX</div>
          <div class="ai-footer-payment-icon-{{ ai_gen_id }}">PP</div>
        </div>
      {% endif %}
    </div>
  </div>
</footer>

