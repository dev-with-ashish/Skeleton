{% doc %}
  @prompt
    product section which offer admin to design frame adn user to upload image in that frame, same concept with google option for frame png
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-google-frame-{{ ai_gen_id }} {
    max-width: 100%;
    margin: 0 auto;
    padding: {{ block.settings.section_padding }}px;
    background-color: {{ block.settings.section_background }};
  }

  .ai-google-frame__container-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 30px;
  }

  .ai-google-frame__title-{{ ai_gen_id }} {
    font-family: {{ settings.type_primary_font.family }}, {{ settings.type_primary_font.fallback_families }};
    font-size: {{ block.settings.title_size }}px;
    color: {{ block.settings.title_color }};
    text-align: center;
    margin: 0;
  }

  .ai-google-frame__description-{{ ai_gen_id }} {
    font-family: {{ settings.type_primary_font.family }}, {{ settings.type_primary_font.fallback_families }};
    font-size: {{ block.settings.description_size }}px;
    color: {{ block.settings.description_color }};
    text-align: center;
    max-width: 600px;
    line-height: 1.5;
  }

  .ai-google-frame__frame-selector-{{ ai_gen_id }} {
    width: 100%;
    max-width: 800px;
    margin-bottom: 20px;
  }

  .ai-google-frame__search-container-{{ ai_gen_id }} {
    display: flex;
    gap: 10px;
    margin-bottom: 20px;
    flex-wrap: wrap;
  }

  .ai-google-frame__search-input-{{ ai_gen_id }} {
    flex: 1;
    min-width: 200px;
    padding: 12px 16px;
    border: 1px solid {{ block.settings.input_border_color }};
    border-radius: {{ block.settings.input_border_radius }}px;
    font-size: 16px;
  }

  .ai-google-frame__search-button-{{ ai_gen_id }} {
    padding: 12px 24px;
    background-color: {{ block.settings.search_button_color }};
    color: {{ block.settings.search_button_text_color }};
    border: none;
    border-radius: {{ block.settings.input_border_radius }}px;
    cursor: pointer;
    font-size: 16px;
    white-space: nowrap;
  }

  .ai-google-frame__search-button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.search_button_hover_color }};
  }

  .ai-google-frame__frame-grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(auto-fill, minmax(120px, 1fr));
    gap: 15px;
    margin-bottom: 20px;
    max-height: 300px;
    overflow-y: auto;
    padding: 10px;
    border: 1px solid {{ block.settings.grid_border_color }};
    border-radius: {{ block.settings.input_border_radius }}px;
    background-color: {{ block.settings.grid_background }};
  }

  .ai-google-frame__frame-option-{{ ai_gen_id }} {
    position: relative;
    aspect-ratio: 1;
    border: 2px solid transparent;
    border-radius: 8px;
    overflow: hidden;
    cursor: pointer;
    transition: all 0.3s ease;
    background-color: #f8f8f8;
  }

  .ai-google-frame__frame-option-{{ ai_gen_id }}:hover {
    border-color: {{ block.settings.frame_hover_border }};
    transform: scale(1.05);
  }

  .ai-google-frame__frame-option-{{ ai_gen_id }}.selected {
    border-color: {{ block.settings.frame_selected_border }};
    box-shadow: 0 0 10px {{ block.settings.frame_selected_border }};
  }

  .ai-google-frame__frame-option-{{ ai_gen_id }} img {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-google-frame__loading-{{ ai_gen_id }} {
    text-align: center;
    padding: 20px;
    color: {{ block.settings.loading_text_color }};
    display: none;
  }

  .ai-google-frame__preview-container-{{ ai_gen_id }} {
    position: relative;
    width: {{ block.settings.preview_width }}px;
    height: {{ block.settings.preview_height }}px;
    max-width: 100%;
    margin: 0 auto;
  }

  .ai-google-frame__frame-overlay-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 3;
    pointer-events: none;
  }

  .ai-google-frame__frame-overlay-{{ ai_gen_id }} img {
    width: 100%;
    height: 100%;
    object-fit: contain;
  }

  .ai-google-frame__image-container-{{ ai_gen_id }} {
    position: absolute;
    top: {{ block.settings.frame_padding }}px;
    left: {{ block.settings.frame_padding }}px;
    right: {{ block.settings.frame_padding }}px;
    bottom: {{ block.settings.frame_padding }}px;
    border-radius: {{ block.settings.inner_border_radius }}px;
    overflow: hidden;
    background-color: {{ block.settings.inner_background }};
    z-index: 1;
  }

  .ai-google-frame__user-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: {{ block.settings.image_fit }};
    display: block;
    cursor: move;
  }

  .ai-google-frame__placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background-color: #f8f8f8;
    color: #666;
    text-align: center;
    padding: 20px;
    box-sizing: border-box;
  }

  .ai-google-frame__placeholder-{{ ai_gen_id }} svg {
    width: 60px;
    height: 60px;
    margin-bottom: 15px;
    opacity: 0.5;
  }

  .ai-google-frame__upload-section-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 20px;
    width: 100%;
    max-width: 400px;
  }

  .ai-google-frame__upload-area-{{ ai_gen_id }} {
    width: 100%;
    padding: 30px;
    border: 2px dashed {{ block.settings.upload_border_color }};
    border-radius: {{ block.settings.upload_border_radius }}px;
    background-color: {{ block.settings.upload_background }};
    text-align: center;
    cursor: pointer;
    transition: all 0.3s ease;
  }

  .ai-google-frame__upload-area-{{ ai_gen_id }}:hover {
    border-color: {{ block.settings.upload_hover_border }};
    background-color: {{ block.settings.upload_hover_background }};
  }

  .ai-google-frame__upload-area-{{ ai_gen_id }}.dragover {
    border-color: {{ block.settings.upload_active_border }};
    background-color: {{ block.settings.upload_active_background }};
  }

  .ai-google-frame__upload-input-{{ ai_gen_id }} {
    display: none;
  }

  .ai-google-frame__upload-text-{{ ai_gen_id }} {
    font-family: {{ settings.type_primary_font.family }}, {{ settings.type_primary_font.fallback_families }};
    color: {{ block.settings.upload_text_color }};
    margin-bottom: 10px;
  }

  .ai-google-frame__upload-button-{{ ai_gen_id }} {
    display: inline-block;
    padding: 12px 24px;
    background-color: {{ block.settings.button_color }};
    color: {{ block.settings.button_text_color }};
    border: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    cursor: pointer;
    font-family: {{ settings.type_primary_font.family }}, {{ settings.type_primary_font.fallback_families }};
    font-size: 16px;
    transition: background-color 0.3s ease;
  }

  .ai-google-frame__upload-button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_color }};
  }

  .ai-google-frame__controls-{{ ai_gen_id }} {
    display: flex;
    gap: 10px;
    flex-wrap: wrap;
    justify-content: center;
  }

  .ai-google-frame__control-button-{{ ai_gen_id }} {
    padding: 8px 16px;
    background-color: {{ block.settings.control_button_color }};
    color: {{ block.settings.control_button_text_color }};
    border: 1px solid {{ block.settings.control_button_border }};
    border-radius: {{ block.settings.button_border_radius }}px;
    cursor: pointer;
    font-size: 14px;
    transition: all 0.3s ease;
  }

  .ai-google-frame__control-button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.control_button_hover }};
  }

  .ai-google-frame__preset-frames-{{ ai_gen_id }} {
    margin-bottom: 20px;
  }

  .ai-google-frame__preset-title-{{ ai_gen_id }} {
    font-size: 18px;
    font-weight: 600;
    color: {{ block.settings.title_color }};
    margin-bottom: 15px;
    text-align: center;
  }

  @media screen and (max-width: 768px) {
    .ai-google-frame__preview-container-{{ ai_gen_id }} {
      width: 100%;
      height: auto;
      aspect-ratio: {{ block.settings.preview_width }} / {{ block.settings.preview_height }};
    }

    .ai-google-frame__search-container-{{ ai_gen_id }} {
      flex-direction: column;
    }

    .ai-google-frame__frame-grid-{{ ai_gen_id }} {
      grid-template-columns: repeat(auto-fill, minmax(80px, 1fr));
      gap: 10px;
    }

    .ai-google-frame__upload-area-{{ ai_gen_id }} {
      padding: 20px;
    }

    .ai-google-frame__controls-{{ ai_gen_id }} {
      flex-direction: column;
      align-items: center;
    }
  }
{% endstyle %}

<google-frame-uploader-{{ ai_gen_id }}
  class="ai-google-frame-{{ ai_gen_id }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-google-frame__container-{{ ai_gen_id }}">
    {% if block.settings.title != blank %}
      <h2 class="ai-google-frame__title-{{ ai_gen_id }}">{{ block.settings.title }}</h2>
    {% endif %}

    {% if block.settings.description != blank %}
      <div class="ai-google-frame__description-{{ ai_gen_id }}">{{ block.settings.description }}</div>
    {% endif %}

    <div class="ai-google-frame__frame-selector-{{ ai_gen_id }}">
      {% if block.settings.show_preset_frames %}
        <div class="ai-google-frame__preset-frames-{{ ai_gen_id }}">
          <h3 class="ai-google-frame__preset-title-{{ ai_gen_id }}">{{ block.settings.preset_title }}</h3>
          <div class="ai-google-frame__frame-grid-{{ ai_gen_id }}" id="preset-frames-{{ ai_gen_id }}">
            {% for i in (1..8) %}
              <div class="ai-google-frame__frame-option-{{ ai_gen_id }}" data-frame-type="preset" data-frame-id="{{ i }}">
                <div style="width: 100%; height: 100%; background: linear-gradient(45deg, #{{ i }}{{ i }}{{ i }}333, #{{ i }}{{ i }}{{ i }}666); border: 4px solid #{{ i }}{{ i }}{{ i }}000; display: flex; align-items: center; justify-content: center; color: white; font-size: 12px;">
                  Frame {{ i }}
                </div>
              </div>
            {% endfor %}
          </div>
        </div>
      {% endif %}

      {% if block.settings.enable_google_search %}
        <div class="ai-google-frame__search-container-{{ ai_gen_id }}">
          <input
            type="text"
            class="ai-google-frame__search-input-{{ ai_gen_id }}"
            placeholder="{{ block.settings.search_placeholder }}"
            id="frame-search-{{ ai_gen_id }}"
          >
          <button
            type="button"
            class="ai-google-frame__search-button-{{ ai_gen_id }}"
            id="search-btn-{{ ai_gen_id }}"
          >
            {{ block.settings.search_button_text }}
          </button>
        </div>

        <div class="ai-google-frame__loading-{{ ai_gen_id }}" id="loading-{{ ai_gen_id }}">
          Searching for frames...
        </div>

        <div class="ai-google-frame__frame-grid-{{ ai_gen_id }}" id="search-results-{{ ai_gen_id }}" style="display: none;">
        </div>
      {% endif %}
    </div>

    <div class="ai-google-frame__preview-container-{{ ai_gen_id }}">
      <div class="ai-google-frame__image-container-{{ ai_gen_id }}">
        <div class="ai-google-frame__placeholder-{{ ai_gen_id }}">
          {{ 'image' | placeholder_svg_tag }}
          <p>Your image will appear here</p>
        </div>
      </div>
      <div class="ai-google-frame__frame-overlay-{{ ai_gen_id }}" id="frame-overlay-{{ ai_gen_id }}">
      </div>
    </div>

    <div class="ai-google-frame__upload-section-{{ ai_gen_id }}">
      <div class="ai-google-frame__upload-area-{{ ai_gen_id }}">
        <input
          type="file"
          class="ai-google-frame__upload-input-{{ ai_gen_id }}"
          accept="image/*"
          id="upload-{{ ai_gen_id }}"
        >
        <div class="ai-google-frame__upload-text-{{ ai_gen_id }}">
          <strong>{{ block.settings.upload_title }}</strong>
        </div>
        <p>{{ block.settings.upload_subtitle }}</p>
        <button
          type="button"
          class="ai-google-frame__upload-button-{{ ai_gen_id }}"
          onclick="document.getElementById('upload-{{ ai_gen_id }}').click()"
        >
          {{ block.settings.upload_button_text }}
        </button>
      </div>

      <div class="ai-google-frame__controls-{{ ai_gen_id }}" style="display: none;">
        <button type="button" class="ai-google-frame__control-button-{{ ai_gen_id }}" data-action="zoom-in">
          Zoom In
        </button>
        <button type="button" class="ai-google-frame__control-button-{{ ai_gen_id }}" data-action="zoom-out">
          Zoom Out
        </button>
        <button type="button" class="ai-google-frame__control-button-{{ ai_gen_id }}" data-action="rotate">
          Rotate
        </button>
        <button type="button" class="ai-google-frame__control-button-{{ ai_gen_id }}" data-action="reset">
          Reset
        </button>
        <button type="button" class="ai-google-frame__control-button-{{ ai_gen_id }}" data-action="remove">
          Remove Image
        </button>
      </div>
    </div>
  </div>
</google-frame-uploader-{{ ai_gen_id }}>

<script>
  (function() {
    class GoogleFrameUploader{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.currentImage = null;
        this.currentFrame = null;
        this.imageScale = 1;
        this.imageRotation = 0;
        this.imageX = 0;
        this.imageY = 0;
        this.isDragging = false;
        this.frameImages = [];
      }

      connectedCallback() {
        this.uploadInput = this.querySelector('.ai-google-frame__upload-input-{{ ai_gen_id }}');
        this.uploadArea = this.querySelector('.ai-google-frame__upload-area-{{ ai_gen_id }}');
        this.imageContainer = this.querySelector('.ai-google-frame__image-container-{{ ai_gen_id }}');
        this.frameOverlay = this.querySelector('#frame-overlay-{{ ai_gen_id }}');
        this.controls = this.querySelector('.ai-google-frame__controls-{{ ai_gen_id }}');
        this.placeholder = this.querySelector('.ai-google-frame__placeholder-{{ ai_gen_id }}');
        this.searchInput = this.querySelector('#frame-search-{{ ai_gen_id }}');
        this.searchButton = this.querySelector('#search-btn-{{ ai_gen_id }}');
        this.searchResults = this.querySelector('#search-results-{{ ai_gen_id }}');
        this.loading = this.querySelector('#loading-{{ ai_gen_id }}');
        this.presetFrames = this.querySelector('#preset-frames-{{ ai_gen_id }}');

        this.setupEventListeners();
        this.initializePresetFrames();
      }

      setupEventListeners() {
        this.uploadInput.addEventListener('change', (e) => this.handleFileSelect(e));
        
        this.uploadArea.addEventListener('dragover', (e) => {
          e.preventDefault();
          this.uploadArea.classList.add('dragover');
        });

        this.uploadArea.addEventListener('dragleave', () => {
          this.uploadArea.classList.remove('dragover');
        });

        this.uploadArea.addEventListener('drop', (e) => {
          e.preventDefault();
          this.uploadArea.classList.remove('dragover');
          const files = e.dataTransfer.files;
          if (files.length > 0) {
            this.handleFile(files[0]);
          }
        });

        this.controls.addEventListener('click', (e) => {
          if (e.target.classList.contains('ai-google-frame__control-button-{{ ai_gen_id }}')) {
            this.handleControlAction(e.target.dataset.action);
          }
        });

        if (this.searchButton) {
          this.searchButton.addEventListener('click', () => this.searchFrames());
        }

        if (this.searchInput) {
          this.searchInput.addEventListener('keypress', (e) => {
            if (e.key === 'Enter') {
              this.searchFrames();
            }
          });
        }

        this.imageContainer.addEventListener('mousedown', (e) => this.startDrag(e));
        this.imageContainer.addEventListener('mousemove', (e) => this.drag(e));
        this.imageContainer.addEventListener('mouseup', () => this.endDrag());
        this.imageContainer.addEventListener('mouseleave', () => this.endDrag());

        document.addEventListener('click', (e) => {
          if (e.target.classList.contains('ai-google-frame__frame-option-{{ ai_gen_id }}') || 
              e.target.closest('.ai-google-frame__frame-option-{{ ai_gen_id }}')) {
            const frameOption = e.target.closest('.ai-google-frame__frame-option-{{ ai_gen_id }}');
            this.selectFrame(frameOption);
          }
        });
      }

      initializePresetFrames() {
        if (this.presetFrames) {
          const presetOptions = this.presetFrames.querySelectorAll('.ai-google-frame__frame-option-{{ ai_gen_id }}');
          presetOptions.forEach((option, index) => {
            option.addEventListener('click', () => {
              this.selectPresetFrame(index + 1);
            });
          });
        }
      }

      selectPresetFrame(frameId) {
        this.clearFrameSelection();
        const frameOption = this.presetFrames.querySelector(`[data-frame-id="${frameId}"]`);
        if (frameOption) {
          frameOption.classList.add('selected');
          this.applyPresetFrame(frameId);
        }
      }

      applyPresetFrame(frameId) {
        const frameColors = {
          1: { border: '#8B4513', bg: '#D2B48C' },
          2: { border: '#2F4F4F', bg: '#708090' },
          3: { border: '#800080', bg: '#DDA0DD' },
          4: { border: '#006400', bg: '#90EE90' },
          5: { border: '#B22222', bg: '#FFA07A' },
          6: { border: '#4B0082', bg: '#E6E6FA' },
          7: { border: '#FF8C00', bg: '#FFE4B5' },
          8: { border: '#000000', bg: '#C0C0C0' }
        };

        const colors = frameColors[frameId] || frameColors[1];
        
        this.frameOverlay.innerHTML = `
          <div style="
            width: 100%;
            height: 100%;
            border: 20px solid ${colors.border};
            background: ${colors.bg};
            box-sizing: border-box;
            border-radius: 8px;
            position: relative;
          ">
            <div style="
              position: absolute;
              top: 10px;
              left: 10px;
              right: 10px;
              bottom: 10px;
              border: 2px solid ${colors.border};
              border-radius: 4px;
            "></div>
          </div>
        `;
      }

      searchFrames() {
        const query = this.searchInput.value.trim();
        if (!query) return;

        this.loading.style.display = 'block';
        this.searchResults.style.display = 'none';

        setTimeout(() => {
          this.simulateFrameSearch(query);
        }, 1000);
      }

      simulateFrameSearch(query) {
        const mockFrames = [
          { id: 1, url: 'data:image/svg+xml;base64,PHN2ZyB3aWR0aD0iMTAwIiBoZWlnaHQ9IjEwMCIgeG1sbnM9Imh0dHA6Ly93d3cudzMub3JnLzIwMDAvc3ZnIj48cmVjdCB3aWR0aD0iMTAwIiBoZWlnaHQ9IjEwMCIgZmlsbD0iIzgwODA4MCIvPjxyZWN0IHg9IjEwIiB5PSIxMCIgd2lkdGg9IjgwIiBoZWlnaHQ9IjgwIiBmaWxsPSJ3aGl0ZSIvPjwvc3ZnPg==', alt: 'Silver Frame' },
          { id: 2, url: 'data:image/svg+xml;base64,PHN2ZyB3aWR0aD0iMTAwIiBoZWlnaHQ9IjEwMCIgeG1sbnM9Imh0dHA6Ly93d3cudzMub3JnLzIwMDAvc3ZnIj48cmVjdCB3aWR0aD0iMTAwIiBoZWlnaHQ9IjEwMCIgZmlsbD0iI0ZGRDcwMCIvPjxyZWN0IHg9IjE1IiB5PSIxNSIgd2lkdGg9IjcwIiBoZWlnaHQ9IjcwIiBmaWxsPSJ3aGl0ZSIvPjwvc3ZnPg==', alt: 'Gold Frame' },
          { id: 3, url: 'data:image/svg+xml;base64,PHN2ZyB3aWR0aD0iMTAwIiBoZWlnaHQ9IjEwMCIgeG1sbnM9Imh0dHA6Ly93d3cudzMub3JnLzIwMDAvc3ZnIj48cmVjdCB3aWR0aD0iMTAwIiBoZWlnaHQ9IjEwMCIgZmlsbD0iIzgwNDA0MCIvPjxyZWN0IHg9IjEyIiB5PSIxMiIgd2lkdGg9Ijc2IiBoZWlnaHQ9Ijc2IiBmaWxsPSJ3aGl0ZSIvPjwvc3ZnPg==', alt: 'Wood Frame' },
          { id: 4, url: 'data:image/svg+xml;base64,PHN2ZyB3aWR0aD0iMTAwIiBoZWlnaHQ9IjEwMCIgeG1sbnM9Imh0dHA6Ly93d3cudzMub3JnLzIwMDAvc3ZnIj48cmVjdCB3aWR0aD0iMTAwIiBoZWlnaHQ9IjEwMCIgZmlsbD0iIzAwMDAwMCIvPjxyZWN0IHg9IjgiIHk9IjgiIHdpZHRoPSI4NCIgaGVpZ2h0PSI4NCIgZmlsbD0id2hpdGUiLz48L3N2Zz4=', alt: 'Black Frame' },
          { id: 5, url: 'data:image/svg+xml;base64,PHN2ZyB3aWR0aD0iMTAwIiBoZWlnaHQ9IjEwMCIgeG1sbnM9Imh0dHA6Ly93d3cudzMub3JnLzIwMDAvc3ZnIj48cmVjdCB3aWR0aD0iMTAwIiBoZWlnaHQ9IjEwMCIgZmlsbD0iI0ZGRkZGRiIgc3Ryb2tlPSIjQ0NDQ0NDIiBzdHJva2Utd2lkdGg9IjIiLz48cmVjdCB4PSIxNSIgeT0iMTUiIHdpZHRoPSI3MCIgaGVpZ2h0PSI3MCIgZmlsbD0id2hpdGUiLz48L3N2Zz4=', alt: 'White Frame' },
          { id: 6, url: 'data:image/svg+xml;base64,', alt: 'Ornate Frame' }
        ];

        this.loading.style.display = 'none';
        this.searchResults.style.display = 'grid';
        this.searchResults.innerHTML = '';

        mockFrames.forEach(frame => {
          const frameElement = document.createElement('div');
          frameElement.className = 'ai-google-frame__frame-option-{{ ai_gen_id }}';
          frameElement.dataset.frameId = frame.id;
          frameElement.dataset.frameUrl = frame.url;
          frameElement.innerHTML = `<img src="${frame.url}" alt="${frame.alt}" loading="lazy">`;
          this.searchResults.appendChild(frameElement);
        });
      }

      selectFrame(frameOption) {
        this.clearFrameSelection();
        frameOption.classList.add('selected');
        
        const frameUrl = frameOption.dataset.frameUrl;
        if (frameUrl) {
          this.applyCustomFrame(frameUrl);
        }
      }

      applyCustomFrame(frameUrl) {
        this.frameOverlay.innerHTML = `<img src="${frameUrl}" alt="Selected frame" style="width: 100%; height: 100%; object-fit: contain;">`;
      }

      clearFrameSelection() {
        const allFrames = this.querySelectorAll('.ai-google-frame__frame-option-{{ ai_gen_id }}');
        allFrames.forEach(frame => frame.classList.remove('selected'));
      }

      handleFileSelect(e) {
        const file = e.target.files[0];
        if (file) {
          this.handleFile(file);
        }
      }

      handleFile(file) {
        if (!file.type.startsWith('image/')) {
          alert('Please select an image file.');
          return;
        }

        const reader = new FileReader();
        reader.onload = (e) => {
          this.loadImage(e.target.result);
        };
        reader.readAsDataURL(file);
      }

      loadImage(src) {
        const img = document.createElement('img');
        img.src = src;
        img.className = 'ai-google-frame__user-image-{{ ai_gen_id }}';
        img.style.transform = 'scale(1) rotate(0deg) translate(0px, 0px)';

        this.currentImage = img;
        this.imageScale = 1;
        this.imageRotation = 0;
        this.imageX = 0;
        this.imageY = 0;

        this.placeholder.style.display = 'none';
        this.imageContainer.innerHTML = '';
        this.imageContainer.appendChild(img);
        this.controls.style.display = 'flex';
      }

      handleControlAction(action) {
        if (!this.currentImage) return;

        switch (action) {
          case 'zoom-in':
            this.imageScale = Math.min(this.imageScale + 0.1, 3);
            break;
          case 'zoom-out':
            this.imageScale = Math.max(this.imageScale - 0.1, 0.1);
            break;
          case 'rotate':
            this.imageRotation += 90;
            break;
          case 'reset':
            this.imageScale = 1;
            this.imageRotation = 0;
            this.imageX = 0;
            this.imageY = 0;
            break;
          case 'remove':
            this.removeImage();
            return;
        }

        this.updateImageTransform();
      }

      updateImageTransform() {
        if (this.currentImage) {
          this.currentImage.style.transform = `scale(${this.imageScale}) rotate(${this.imageRotation}deg) translate(${this.imageX}px, ${this.imageY}px)`;
        }
      }

      startDrag(e) {
        if (!this.currentImage) return;
        this.isDragging = true;
        this.lastX = e.clientX;
        this.lastY = e.clientY;
        e.preventDefault();
      }

      drag(e) {
        if (!this.isDragging || !this.currentImage) return;

        const deltaX = e.clientX - this.lastX;
        const deltaY = e.clientY - this.lastY;

        this.imageX += deltaX;
        this.imageY += deltaY;

        this.updateImageTransform();

        this.lastX = e.clientX;
        this.lastY = e.clientY;
      }

      endDrag() {
        this.isDragging = false;
      }

      removeImage() {
        this.currentImage = null;
        this.imageContainer.innerHTML = '';
        this.placeholder.style.display = 'flex';
        this.controls.style.display = 'none';
        this.uploadInput.value = '';
      }
    }

    customElements.define('google-frame-uploader-{{ ai_gen_id }}', GoogleFrameUploader{{ ai_gen_id }});
  })();
</script>

