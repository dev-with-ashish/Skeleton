      {% for slide in services %}
        {% assign slide_services = slide | split: ',' %}
        <div class="ai-services-slide-{{ ai_gen_id }} {% if forloop.first %}active{% endif %}" data-slide="{{ forloop.index0 }}">
          {% for service in slide_services %}
            <div 
              class="ai-service-item-{{ ai_gen_id }} {% if forloop.first and forloop.parentloop.first %}selected{% endif %}"
              data-service="{{ service | strip | handle }}"
              data-slide="{{ forloop.parentloop.index0 }}"
            >
              {{ service | strip }}
            </div>
          {% endfor %}
        </div>
      {% endfor %}<div class="ai-services-navigation-{{ ai_gen_id }}">
        <button
          class="ai-services-arrow-{{ ai_gen_id }} ai-services-arrow-prev-{{ ai_gen_id }}"
          aria-label="Previous slide"
          disabled
        >
          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 19l-7-7 7-7" />
          </svg>
        </button>

        <div class="ai-services-nav-dots-{{ ai_gen_id }}">
          {% for slide in services %}
            <div
              class="ai-services-nav-dot-{{ ai_gen_id }} {% if forloop.first %}active{% endif %}"
              data-slide="{{ forloop.index0 }}"></div>
          {% endfor %}
        </div>

        <button 
          class="ai-services-arrow-{{ ai_gen_id }} ai-services-arrow-next-{{ ai_gen_id }}"
          aria-label="Next slide"
        >
          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
          </svg>
        </button>
