{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-services-section-{{ ai_gen_id }} {
    padding: 60px 20px;
    background-color: {{ block.settings.background_color }};
    max-width: 1200px;
    margin: 0 auto;
  }

  .ai-services-container-{{ ai_gen_id }} {
    display: flex;
    gap: 40px;
    align-items: stretch;
    min-height: 500px;
  }

  .ai-services-list-{{ ai_gen_id }} {
    flex: 1;
    background-color: {{ block.settings.list_background_color }};
    border-radius: {{ block.settings.border_radius }}px;
    padding: 30px;
    box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
    position: relative;
  }

  .ai-services-image-container-{{ ai_gen_id }} {
    flex: 1;
    background-color: {{ block.settings.image_background_color }};
    border-radius: {{ block.settings.border_radius }}px;
    overflow: hidden;
    position: relative;
    box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-services-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    font-weight: 700;
    color: {{ block.settings.title_color }};
    margin-bottom: 30px;
    text-align: center;
  }

  .ai-services-slide-{{ ai_gen_id }} {
    display: none;
    animation: ai-services-fade-in-{{ ai_gen_id }} 0.3s ease-in-out;
  }

  .ai-services-slide-{{ ai_gen_id }}.active {
    display: block;
  }

  .ai-service-item-{{ ai_gen_id }} {
    padding: 15px 20px;
    margin-bottom: 12px;
    background-color: {{ block.settings.item_background_color }};
    border-radius: {{ block.settings.item_border_radius }}px;
    cursor: pointer;
    transition: all 0.3s ease;
    border: 2px solid {{ block.settings.item_border_color }};
    color: {{ block.settings.item_text_color }};
    font-size: {{ block.settings.item_text_size }}px;
    font-weight: 500;
  }

  .ai-service-item-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.item_hover_background_color }};
    color: {{ block.settings.item_hover_text_color }};
    border-color: {{ block.settings.item_hover_border_color }};
    transform: translateX(8px);
    box-shadow: 0 4px 15px rgba(0, 0, 0, 0.15);
  }

  .ai-service-item-{{ ai_gen_id }}.selected {
    background-color: {{ block.settings.item_selected_background_color }};
    color: {{ block.settings.item_hover_text_color }};
    border-color: {{ block.settings.item_selected_border_color }};
    transform: translateX(8px);
    box-shadow: 0 4px 15px rgba(0, 0, 0, 0.15);
  }

  .ai-services-navigation-{{ ai_gen_id }} {
    display: flex;
    justify-content: center;
    align-items: center;
    gap: 15px;
    margin-top: 25px;
  }

  .ai-services-arrow-{{ ai_gen_id }} {
    width: 40px;
    height: 40px;
    border-radius: 50%;
    background-color: {{ block.settings.arrow_background_color }};
    border: none;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: all 0.3s ease;
    color: {{ block.settings.arrow_color }};
  }

  .ai-services-arrow-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.arrow_hover_background_color }};
    color: {{ block.settings.arrow_hover_color }};
    transform: scale(1.1);
  }

  .ai-services-arrow-{{ ai_gen_id }}:disabled {
    opacity: 0.4;
    cursor: not-allowed;transform: none;
  }

  .ai-services-arrow-{{ ai_gen_id }} svg {
    width: 20px;
    height: 20px;
  }

  .ai-services-nav-dots-{{ ai_gen_id }} {
    display: flex;
    gap: 8px;}

  .ai-services-nav-dot-{{ ai_gen_id }} {
    width: 12px;
    height: 12px;
    border-radius: 50%;
    background-color: {{ block.settings.nav_inactive_color }};
    cursor: pointer;
    transition: all 0.3s ease;
  }

  .ai-services-nav-dot-{{ ai_gen_id }}.active {
    background-color: {{ block.settings.accent_color }};transform: scale(1.2);
  }

  .ai-services-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: opacity 0.4s ease;
    opacity: 0;position: absolute;
    top: 0;
    left: 0;
  }

  .ai-services-image-{{ ai_gen_id }}.active {
    opacity: 1;
  }

  .ai-services-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #f8f9fa;
    color: #6c757d;
    font-size: 18px;
    position: absolute;
    top: 0;
    left: 0;}

  .ai-services-placeholder-{{ ai_gen_id }} svg {
    width: 120px;
    height: 120px;opacity: 0.5;
  }

  @keyframes ai-services-fade-in-{{ ai_gen_id }} {
    from {
      opacity: 0;
      transform: translateY(10px);
    }
    to {
      opacity: 1;
      transform: translateY(0);
    }
  }

  @media screen and (max-width: 768px) {
    .ai-services-container-{{ ai_gen_id }} {
      flex-direction: column;
      gap: 20px;
    }

    .ai-services-section-{{ ai_gen_id }} {
      padding: 40px 15px;
    }

    .ai-services-list-{{ ai_gen_id }},
    .ai-services-image-container-{{ ai_gen_id }} {
      min-height: 300px;
    }

    .ai-services-arrow-{{ ai_gen_id }} {
      width: 36px;
      height: 36px;
    }

    .ai-services-arrow-{{ ai_gen_id }} svg {
      width: 18px;
      height: 18px;
    }
  }
{% endstyle %}

<services-section-{{ ai_gen_id }}
