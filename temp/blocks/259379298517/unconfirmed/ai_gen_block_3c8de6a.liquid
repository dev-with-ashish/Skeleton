<script>
  (function() {
    class ServicesSection{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.currentSlide = 0;
        this.totalSlides = 0;
        this.selectedService = 'events';
        this.hoveredService = null;}

      connectedCallback() {
        this.slides = this.querySelectorAll('.ai-services-slide-{{ ai_gen_id }}');
        this.navDots = this.querySelectorAll('.ai-services-nav-dot-{{ ai_gen_id }}');
        this.serviceItems = this.querySelectorAll('.ai-service-item-{{ ai_gen_id }}');
        this.images = this.querySelectorAll('.ai-services-image-{{ ai_gen_id }}');
        this.placeholder = this.querySelector('.ai-services-placeholder-{{ ai_gen_id }}');
        this.prevArrow = this.querySelector('.ai-services-arrow-prev-{{ ai_gen_id }}');
        this.nextArrow = this.querySelector('.ai-services-arrow-next-{{ ai_gen_id }}');
        
        this.totalSlides = this.slides.length;
        this.setupEventListeners();
        this.updateArrowStates();
        this.showImage(this.selectedService);
      }

      setupEventListeners() {
        this.serviceItems.forEach(item => {
          item.addEventListener('click', () => this.handleServiceClick(item));item.addEventListener('mouseenter', () => this.handleServiceHover(item));
          item.addEventListener('mouseleave', () => this.handleServiceLeave(item));
        });

        this.navDots.forEach(dot => {
          dot.addEventListener('click', () => this.handleDotClick(dot));
        });

        this.prevArrow.addEventListener('click', () => this.goToPrevSlide());
        this.nextArrow.addEventListener('click', () => this.goToNextSlide());

        if ('ontouchstart' in window) {
          this.serviceItems.forEach(item => {
            item.addEventListener('touchstart', () => this.handleServiceClick(item));
          });
        }document.addEventListener('keydown', (e) => {
          if (e.key === 'ArrowLeft') {
            this.goToPrevSlide();
          } else if (e.key === 'ArrowRight') {
            this.goToNextSlide();
          }
        });
      }

      handleServiceClick(item) {
        const serviceHandle = item.dataset.service;
        const slideIndex = parseInt(item.dataset.slide);
        
        this.serviceItems.forEach(si => si.classList.remove('selected'));
        item.classList.add('selected');
        
        this.selectedService = serviceHandle;
        this.showImage(serviceHandle);
        if (slideIndex !== this.currentSlide) {
          this.switchSlide(slideIndex);
        }
      }

      handleServiceHover(item) {
        const serviceHandle = item.dataset.service;
        this.hoveredService = serviceHandle;
        this.showImage(serviceHandle);
      }

      handleServiceLeave(item) {
        this.hoveredService = null;
        this.showImage(this.selectedService);
      }

      handleDotClick(dot) {
        const slideIndex = parseInt(dot.dataset.slide);
        this.switchSlide(slideIndex);
      }

      goToPrevSlide() {
        if (this.currentSlide > 0) {
          this.switchSlide(this.currentSlide - 1);
        }
      }

      goToNextSlide() {
        if (this.currentSlide < this.totalSlides - 1) {
          this.switchSlide(this.currentSlide + 1);
        }
      }

      switchSlide(slideIndex) {
        if (slideIndex < 0 || slideIndex >= this.totalSlides) return;
        
        this.slides[this.currentSlide].classList.remove('active');
        this.navDots[this.currentSlide].classList.remove('active');
        
        this.currentSlide = slideIndex;
        
        this.slides[this.currentSlide].classList.add('active');
        this.navDots[this.currentSlide].classList.add('active');
        
        const firstItemInSlide = this.slides[this.currentSlide].querySelector('.ai-service-item-{{ ai_gen_id }}');
        if (firstItemInSlide && !this.slides[this.currentSlide].querySelector('.ai-service-item-{{ ai_gen_id }}.selected')) {
          this.serviceItems.forEach(item => item.classList.remove('selected'));
          firstItemInSlide.classList.add('selected');
          this.selectedService = firstItemInSlide.dataset.service;
          this.showImage(this.selectedService);
        }
        this.updateArrowStates();
      }

      updateArrowStates() {
        this.prevArrow.disabled = this.currentSlide === 0;
        this.nextArrow.disabled = this.currentSlide === this.totalSlides - 1;
      }

      showImage(serviceHandle) {
        this.images.forEach(img => img.classList.remove('active'));
        this.placeholder.style.display = 'flex';
        
        const targetImage = this.querySelector(`img[data-service="${serviceHandle}"]`);
        if (targetImage) {
          targetImage.classList.add('active');
          this.placeholder.style.display = 'none';
        }
      }
    }

    customElements.define('services-section-{{ ai_gen_id }}', ServicesSection{{ ai_gen_id }});
  })();
</script>

