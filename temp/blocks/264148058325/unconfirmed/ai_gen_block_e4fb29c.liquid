{% doc %}
  @prompt
    a section for my acrylic page showing 15 different category with title each category with option to add multiple product (product title, product image, and customize now button below it) make according to you best styled and attractive dark theme
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-acrylic-categories-{{ ai_gen_id }} {
    background: linear-gradient(135deg, #1a1a1a 0%, #2d2d2d 100%);
    padding: {{ block.settings.section_padding }}px 20px;
    min-height: 100vh;
  }

  .ai-acrylic-categories-container-{{ ai_gen_id }} {
    max-width: 1400px;
    margin: 0 auto;
  }

  .ai-acrylic-categories-header-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 60px;
  }

  .ai-acrylic-categories-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.main_title_size }}px;
    font-weight: 700;
    color: {{ block.settings.title_color }};
    margin: 0 0 20px;
    text-transform: uppercase;
    letter-spacing: 2px;
    background: linear-gradient(45deg, {{ block.settings.title_color }}, {{ block.settings.accent_color }});
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
  }

  .ai-acrylic-categories-subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.subtitle_size }}px;
    color: {{ block.settings.subtitle_color }};
    margin: 0;
    opacity: 0.9;
  }

  .ai-acrylic-category-{{ ai_gen_id }} {
    margin-bottom: 80px;
    padding: 40px;
    background: rgba(255, 255, 255, 0.05);
    border-radius: {{ block.settings.category_border_radius }}px;
    border: 1px solid rgba(255, 255, 255, 0.1);
    backdrop-filter: blur(10px);
    transition: all 0.3s ease;
  }

  .ai-acrylic-category-{{ ai_gen_id }}:hover {
    background: rgba(255, 255, 255, 0.08);
    border-color: {{ block.settings.accent_color }};
    transform: translateY(-5px);
    box-shadow: 0 20px 40px rgba(0, 0, 0, 0.3);
  }

  .ai-acrylic-category-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.category_title_size }}px;
    font-weight: 600;
    color: {{ block.settings.category_title_color }};
    margin: 0 0 30px;
    text-align: center;
    text-transform: uppercase;
    letter-spacing: 1px;
    position: relative;
  }

  .ai-acrylic-category-title-{{ ai_gen_id }}::after {
    content: '';
    position: absolute;
    bottom: -10px;
    left: 50%;
    transform: translateX(-50%);
    width: 60px;
    height: 3px;
    background: linear-gradient(90deg, {{ block.settings.accent_color }}, {{ block.settings.title_color }});
    border-radius: 2px;
  }

  .ai-acrylic-products-grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
    gap: 30px;
  }

  .ai-acrylic-product-card-{{ ai_gen_id }} {
    background: rgba(0, 0, 0, 0.4);
    border-radius: {{ block.settings.product_border_radius }}px;
    padding: 25px;
    border: 1px solid rgba(255, 255, 255, 0.1);
    transition: all 0.3s ease;
    position: relative;
    overflow: hidden;
  }

  .ai-acrylic-product-card-{{ ai_gen_id }}::before {
    content: '';
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.1), transparent);
    transition: left 0.5s ease;
  }

  .ai-acrylic-product-card-{{ ai_gen_id }}:hover::before {
    left: 100%;
  }

  .ai-acrylic-product-card-{{ ai_gen_id }}:hover {
    background: rgba(0, 0, 0, 0.6);
    border-color: {{ block.settings.accent_color }};
    transform: translateY(-8px);
    box-shadow: 0 15px 30px rgba(0, 0, 0, 0.4);
  }

  .ai-acrylic-product-image-wrapper-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    height: 200px;
    margin-bottom: 20px;
    border-radius: {{ block.settings.image_border_radius }}px;
    overflow: hidden;
    background: rgba(255, 255, 255, 0.05);
  }

  .ai-acrylic-product-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.3s ease;
  }

  .ai-acrylic-product-card-{{ ai_gen_id }}:hover .ai-acrylic-product-image-{{ ai_gen_id }} {
    transform: scale(1.1);
  }

  .ai-acrylic-product-image-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background: rgba(255, 255, 255, 0.05);
    position: relative;
  }

  .ai-acrylic-product-image-placeholder-{{ ai_gen_id }} svg {
    width: 60px;
    height: 60px;
    opacity: 0.3;
    color: {{ block.settings.subtitle_color }};
  }

  .ai-acrylic-product-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.product_title_size }}px;
    font-weight: 500;
    color: {{ block.settings.product_title_color }};
    margin: 0 0 20px;
    text-align: center;
    line-height: 1.4;
  }

  .ai-acrylic-customize-button-{{ ai_gen_id }} {
    width: 100%;
    padding: 12px 24px;
    background: linear-gradient(45deg, {{ block.settings.button_color }}, {{ block.settings.accent_color }});
    color: {{ block.settings.button_text_color }};
    border: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    font-size: {{ block.settings.button_text_size }}px;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 1px;
    cursor: pointer;
    transition: all 0.3s ease;
    text-decoration: none;
    display: inline-block;
    text-align: center;
    position: relative;
    overflow: hidden;
  }

  .ai-acrylic-customize-button-{{ ai_gen_id }}::before {
    content: '';
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background: linear-gradient(45deg, {{ block.settings.accent_color }}, {{ block.settings.button_color }});
    transition: left 0.3s ease;
    z-index: -1;
  }

  .ai-acrylic-customize-button-{{ ai_gen_id }}:hover::before {
    left: 0;
  }

  .ai-acrylic-customize-button-{{ ai_gen_id }}:hover {
    transform: translateY(-2px);
    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.3);
  }

  .ai-acrylic-empty-state-{{ ai_gen_id }} {
    text-align: center;
    padding: 40px 20px;
    color: {{ block.settings.subtitle_color }};
    opacity: 0.7;
    font-style: italic;
  }

  @media screen and (max-width: 768px) {
    .ai-acrylic-categories-{{ ai_gen_id }} {
      padding: {{ block.settings.section_padding | times: 0.7 }}px 15px;
    }

    .ai-acrylic-category-{{ ai_gen_id }} {
      padding: 25px 20px;
      margin-bottom: 50px;
    }

    .ai-acrylic-products-grid-{{ ai_gen_id }} {
      grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
      gap: 20px;
    }

    .ai-acrylic-categories-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.main_title_size | times: 0.8 }}px;
    }

    .ai-acrylic-category-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.category_title_size | times: 0.9 }}px;
    }
  }
{% endstyle %}

<div class="ai-acrylic-categories-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-acrylic-categories-container-{{ ai_gen_id }}">
    {% if block.settings.main_title != blank %}
      <div class="ai-acrylic-categories-header-{{ ai_gen_id }}">
        <h1 class="ai-acrylic-categories-title-{{ ai_gen_id }}">{{ block.settings.main_title }}</h1>
        {% if block.settings.subtitle != blank %}
          <p class="ai-acrylic-categories-subtitle-{{ ai_gen_id }}">{{ block.settings.subtitle }}</p>
        {% endif %}
      </div>
    {% endif %}

    {% for i in (1..15) %}
      {% liquid
        assign category_title_key = 'category_' | append: i | append: '_title'
        assign category_title = block.settings[category_title_key]
      %}

      {% if category_title != blank %}
        <div class="ai-acrylic-category-{{ ai_gen_id }}">
          <h2 class="ai-acrylic-category-title-{{ ai_gen_id }}">{{ category_title }}</h2>
          
          <div class="ai-acrylic-products-grid-{{ ai_gen_id }}">
            {% liquid
              assign has_products = false
              for j in (1..6)
                assign product_key = 'category_' | append: i | append: '_product_' | append: j
                assign product = block.settings[product_key]
                if product != blank
                  assign has_products = true
                  break
                endif
              endfor
            %}

            {% if has_products %}
              {% for j in (1..6) %}
                {% liquid
                  assign product_key = 'category_' | append: i | append: '_product_' | append: j
                  assign product = block.settings[product_key]
                %}

                {% if product != blank %}
                  <div class="ai-acrylic-product-card-{{ ai_gen_id }}">
                    <div class="ai-acrylic-product-image-wrapper-{{ ai_gen_id }}">
                      {% if product.featured_image %}
                        <img
                          src="{{ product.featured_image | image_url: width: 400 }}"
                          alt="{{ product.featured_image.alt | escape }}"
                          class="ai-acrylic-product-image-{{ ai_gen_id }}"
                          loading="lazy"
                          width="400"
                          height="400"
                        >
                      {% else %}
                        <div class="ai-acrylic-product-image-placeholder-{{ ai_gen_id }}">
                          {{ 'product-apparel-1' | placeholder_svg_tag }}
                        </div>
                      {% endif %}
                    </div>
                    
                    <h3 class="ai-acrylic-product-title-{{ ai_gen_id }}">{{ product.title }}</h3>
                    
                    <a
                      href="{{ product.url }}"
                      class="ai-acrylic-customize-button-{{ ai_gen_id }}"
                    >
                      {{ block.settings.button_text }}
                    </a>
                  </div>
                {% endif %}
              {% endfor %}
            {% else %}
              <div class="ai-acrylic-empty-state-{{ ai_gen_id }}">
                Add products to showcase in this category
              </div>
            {% endif %}
          </div>
        </div>
      {% endif %}
    {% endfor %}
  </div>
</div>

