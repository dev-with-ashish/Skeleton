{% doc %}
  @prompt
    generate a about us page according to you using tailwind css and three js the theme should be according to this We design and plan creative event solutions to your requirements. We develop proprietary creative, design, reporting, logistic and technology systems for the widest possible range of event and experiential projects. From launches and press conferences to business conferences, exhibitions, parties and Weddings, we produce events and experiences that enthral and engage the audience., every thing is fine just use this #d91a47 color as base, make sure the cards remain in one line row responsivily

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-about-us-{{ ai_gen_id }} {
    padding: 80px 20px;
    background: linear-gradient(135deg, {{ block.settings.primary_color }} 0%, {{ block.settings.secondary_color }} 100%);
    color: {{ block.settings.text_color }};
    position: relative;
    overflow: hidden;}

  .ai-about-us-container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
    position: relative;
    z-index: 2;
  }

  .ai-about-us-header-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 60px;
  }

  .ai-about-us-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    font-weight: 700;
    margin: 0 0 20px;
    line-height: 1.2;
  }

  .ai-about-us-subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.subtitle_size }}px;
    opacity: 0.9;
    max-width: 600px;
    margin: 0 auto;
    line-height: 1.6;
  }

  .ai-about-us-content-{{ ai_gen_id }} {
    display: flex;
    gap: 30px;
    margin-bottom: 60px;
    overflow-x: auto;
    padding-bottom: 10px;
  }

  .ai-about-us-card-{{ ai_gen_id }} {
    background: rgba(255, 255, 255, {{ block.settings.card_opacity }});
    padding: 40px 30px;
    border-radius: {{ block.settings.card_border_radius }}px;
    backdrop-filter: blur(10px);
    border: 1px solid rgba(255, 255, 255, 0.2);
    transition: transform 0.3s ease, box-shadow 0.3s ease;cursor: pointer;
    flex: 1;
    min-width: 280px;
    max-width: 350px;
  }

  .ai-about-us-card-{{ ai_gen_id }}:hover {
    transform: translateY(-10px);
    box-shadow: 0 20px 40px rgba(0, 0, 0, 0.1);
  }

  .ai-about-us-card-icon-{{ ai_gen_id }} {
    width: 60px;
    height: 60px;
    background: {{ block.settings.icon_color }};
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 0 auto 20px;
    color: white;
  }

  .ai-about-us-card-title-{{ ai_gen_id }} {
    font-size: 24px;
    font-weight: 600;
    margin: 0 0 15px;
    text-align: center;color: {{ block.settings.card_title_color }};
  }

  .ai-about-us-card-text-{{ ai_gen_id }} {
    font-size: 16px;
    line-height: 1.6;
    text-align: center;
    color: {{ block.settings.card_text_color }};
  }

  .ai-about-us-stats-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: 30px;
    text-align: center;}

  .ai-about-us-stat-{{ ai_gen_id }} {
    padding: 20px;}

  .ai-about-us-stat-number-{{ ai_gen_id }} {
    font-size: 48px;
    font-weight: 700;
    margin: 0 0 10px;
    color: {{ block.settings.stat_number_color }};
  }

  .ai-about-us-stat-label-{{ ai_gen_id }} {
    font-size: 18px;
    opacity: 0.9;
    margin: 0;}

  .ai-about-us-background-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
    opacity: 0.1;
  }

  .ai-about-us-particle-{{ ai_gen_id }} {
    position: absolute;
    width: 4px;
    height: 4px;
    background: white;
    border-radius: 50%;
    opacity: 0.6;
  }

  @media screen and (max-width: 1024px) {
    .ai-about-us-card-{{ ai_gen_id }} {
      min-width: 250px;
      max-width: 300px;
      padding: 30px 25px;
    }

    .ai-about-us-content-{{ ai_gen_id }} {
      gap: 20px;
    }
  }

  @media screen and (max-width: 768px) {
    .ai-about-us-{{ ai_gen_id }} {
      padding: 60px 20px;
    }

    .ai-about-us-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.8 }}px;
    }

    .ai-about-us-subtitle-{{ ai_gen_id }} {
      font-size: {{ block.settings.subtitle_size | times: 0.9 }}px;
    }

    .ai-about-us-card-{{ ai_gen_id }} {
      min-width: 220px;
      max-width: 280px;
      padding: 25px 20px;
    }

    .ai-about-us-card-title-{{ ai_gen_id }} {
      font-size: 20px;
    }

    .ai-about-us-card-text-{{ ai_gen_id }} {
      font-size: 14px;
    }

    .ai-about-us-content-{{ ai_gen_id }} {
      gap: 15px;
      margin-bottom: 40px;
    }

    .ai-about-us-stats-{{ ai_gen_id }} {
      grid-template-columns: repeat(2, 1fr);gap: 20px;
    }

    .ai-about-us-stat-number-{{ ai_gen_id }} {
      font-size: 36px;
    }
  }

  @media screen and (max-width: 480px) {
    .ai-about-us-card-{{ ai_gen_id }} {
      min-width: 200px;
      max-width: 250px;
      padding: 20px 15px;
    }

    .ai-about-us-card-icon-{{ ai_gen_id }} {
      width: 50px;
      height: 50px;
    }

    .ai-about-us-card-title-{{ ai_gen_id }} {
      font-size: 18px;
    }

    .ai-about-us-card-text-{{ ai_gen_id }} {
      font-size: 13px;
    }
  }

  .ai-about-us-content-{{ ai_gen_id }}::-webkit-scrollbar {
    height: 6px;
  }

  .ai-about-us-content-{{ ai_gen_id }}::-webkit-scrollbar-track {
    background: rgba(255, 255, 255, 0.1);
    border-radius: 3px;
  }

  .ai-about-us-content-{{ ai_gen_id }}::-webkit-scrollbar-thumb {
    background: rgba(255, 255, 255, 0.3);
    border-radius: 3px;
  }

  .ai-about-us-content-{{ ai_gen_id }}::-webkit-scrollbar-thumb:hover {
    background: rgba(255, 255, 255, 0.5);
  }
{% endstyle %}

<event-about-us-{{ ai_gen_id }}
