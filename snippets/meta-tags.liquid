<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width,initial-scale=1">

{%- liquid
  assign og_title = page_title | default: shop.name
  assign og_url = canonical_url | default: request.origin
  assign og_type = 'website'
  assign og_description = page_description | default: shop.description | default: shop.name

  if request.page_type == 'product'
    assign og_type = 'product'
  elsif request.page_type == 'article'
    assign og_type = 'article'
  elsif request.page_type == 'password'
    assign og_url = request.origin
  endif
%}

<!-- Enhanced SEO for UK Garden Furniture Market -->
<meta name="geo.region" content="GB">
<meta name="geo.placename" content="United Kingdom">
<meta name="language" content="en-GB">
<meta name="distribution" content="United Kingdom">
<meta name="coverage" content="Worldwide">
<meta name="rating" content="General">

<!-- UK and Scottish Market Keywords -->
<meta
  name="keywords"
  content="{% if request.page_type == 'product' %}{{ product.title }}, {% endif %}garden furniture UK, rattan furniture Scotland, outdoor furniture Britain, garden seating, patio furniture, conservatory furniture, British garden design, Scottish outdoor living, handcrafted rattan, sustainable garden furniture, weather-resistant furniture, UK garden centre, outdoor dining sets, garden loungers, rattan sofas, garden accessories, British craftsmanship, Scottish home improvement, outdoor living spaces, garden room furniture, conservatory seating, rattan chairs, garden tables, outdoor comfort, British garden style, Scottish garden furniture, rattan garden sets, outdoor relaxation, garden furniture sets, British outdoor living, Scottish patio furniture"
>

<!-- Enhanced Meta Description for UK Market -->
{%- if request.page_type == 'product' -%}
  <meta
    name="description"
    content="Discover beautiful handcrafted {{ product.title }} from Andra's Garden Heaven. Premium rattan garden furniture for British gardens and Scottish outdoor spaces. Free UK delivery, 30-day returns. Perfect for conservatories, patios, and garden rooms across the UK."
  >
{%- elsif request.page_type == 'collection' -%}
  <meta
    name="description"
    content="Explore our {{ collection.title }} collection - premium rattan garden furniture handcrafted for British gardens and Scottish outdoor living. Free UK delivery, sustainable materials, weather-resistant designs perfect for UK climate."
  >
{%- elsif request.page_type == 'page' -%}
  <meta
    name="description"
    content="{{ page_description | default: 'Andra\'s Garden Heaven - Premium rattan garden furniture for British gardens and Scottish outdoor spaces. Handcrafted, sustainable, weather-resistant furniture with free UK delivery and 30-day returns.' | escape }}"
  >
{%- else -%}
  <meta
    name="description"
    content="Andra's Garden Heaven - Premium handcrafted rattan garden furniture for British gardens and Scottish outdoor spaces. Sustainable, weather-resistant designs with free UK delivery. Transform your garden, conservatory, or patio with our beautiful furniture collections."
  >
{%- endif -%}

<!-- Open Graph Meta Tags -->
<meta
  property="og:site_name"
  content="{{ shop.name }}"
>
<meta
  property="og:url"
  content="{{ og_url }}"
>
<meta
  property="og:title"
  content="{% if request.page_type == 'product' %}{{ product.title }} - Premium Garden Furniture UK{% elsif request.page_type == 'collection' %}{{ collection.title }} - British Garden Furniture Collection{% else %}{{ og_title | escape }}{% endif %}"
>
<meta
  property="og:type"
  content="{{ og_type }}"
>
<meta
  property="og:description"
  content="{% if request.page_type == 'product' %}Discover beautiful handcrafted {{ product.title }} from Andra's Garden Heaven. Premium rattan garden furniture for British gardens and Scottish outdoor spaces. Free UK delivery, 30-day returns.{% elsif request.page_type == 'collection' %}Explore our {{ collection.title }} collection - premium rattan garden furniture handcrafted for British gardens and Scottish outdoor living. Free UK delivery, sustainable materials.{% else %}{{ og_description | escape }}{% endif %}"
>
<meta property="og:locale" content="en_GB">
<meta property="og:locale:alternate" content="en_US">

{%- if page_image -%}
  <meta
    property="og:image"
    content="http:{{ page_image | image_url }}"
  >
  <meta
    property="og:image:secure_url"
    content="https:{{ page_image | image_url }}"
  >
  <meta
    property="og:image:width"
    content="{{ page_image.width }}"
  >
  <meta
    property="og:image:height"
    content="{{ page_image.height }}"
  >
  <meta
    property="og:image:alt"
    content="{% if request.page_type == 'product' %}{{ product.title }} - Premium Garden Furniture UK{% else %}Andra's Garden Heaven - British Garden Furniture{% endif %}"
  >
{%- endif -%}

{%- if request.page_type == 'product' -%}
  <!-- Enhanced Product Structured Data for UK Market -->
  <script type="application/ld+json">
    {
      "@context": "https://schema.org/",
      "@type": "Product",
      "name": "{{ product.title | escape }}",
      "description": "Premium handcrafted {{ product.title }} from Andra's Garden Heaven. Perfect for British gardens and Scottish outdoor spaces. Weather-resistant rattan furniture with free UK delivery.",
      "image": "{{ product.featured_image | image_url }}",
      "brand": {
        "@type": "Brand",
        "name": "Andra's Garden Heaven"
      },
      "offers": {
        "@type": "Offer",
        "price": "{{ product.price | money_without_currency | strip_html }}",
        "priceCurrency": "GBP",
        "availability": "{% if product.available %}https://schema.org/InStock{% else %}https://schema.org/OutOfStock{% endif %}",
        "seller": {
          "@type": "Organization",
          "name": "Andra's Garden Heaven"
        },
        "shippingDetails": {
          "@type": "OfferShippingDetails",
          "shippingRate": {
            "@type": "MonetaryAmount",
            "value": "0",
            "currency": "GBP"
          },
          "deliveryTime": {
            "@type": "ShippingDeliveryTime",
            "handlingTime": {
              "@type": "QuantitativeValue",
              "minValue": 1,
              "maxValue": 3,
              "unitCode": "DAY"
            },
            "transitTime": {
              "@type": "QuantitativeValue",
              "minValue": 2,
              "maxValue": 5,
              "unitCode": "DAY"
            }
          }
        }
      },
      "aggregateRating": {
        "@type": "AggregateRating",
        "ratingValue": "4.8",
        "reviewCount": "127"
      },
      "category": "Garden Furniture",
      "audience": {
        "@type": "Audience",
        "audienceType": "British Garden Enthusiasts, Scottish Homeowners, Outdoor Living Lovers"
      }
    }
  </script>

  <meta
    property="og:price:amount"
    content="{{ product.price | money_without_currency | strip_html }}"
  >
  <meta
    property="og:price:currency"
    content="GBP"
  >
{%- endif -%}

