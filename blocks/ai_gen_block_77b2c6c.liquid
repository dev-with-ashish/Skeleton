{% doc %}
  @prompt
    generate a latest styled responsive TWD The Wedding Diaries Section using tailwind css and three js, use gradiant of #c63b33 this color

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .wedding-diaries-section-{{ ai_gen_id }} {
    position: relative;
    padding: 80px 20px;
    background: linear-gradient(135deg, {{ block.settings.bg_color_1 }} 0%, {{ block.settings.bg_color_2 }} 100%);
    overflow: hidden;
    min-height: 100vh;
    display: flex;
    align-items: center;
    justify-content: center;}

  .wedding-diaries-container-{{ ai_gen_id }} {
    max-width: 1200px;
    width: 100%;
    margin: 0 auto;
    position: relative;z-index: 2;
  }

  .wedding-diaries-content-{{ ai_gen_id }} {
    text-align: center;
    color: {{ block.settings.text_color }};
  }

  .wedding-diaries-badge-{{ ai_gen_id }} {
    display: inline-block;
    padding: 8px 24px;
    background: rgba(198, 59, 51, 0.1);
    border:1px solid rgba(198, 59, 51, 0.3);
    border-radius: 50px;
    font-size: 14px;
    font-weight: 500;
    margin-bottom: 24px;
    backdrop-filter: blur(10px);
    color: {{ block.settings.badge_color }};
  }

  .wedding-diaries-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    font-weight: 700;
    line-height: 1.2;
    margin-bottom: 24px;
    background: linear-gradient(45deg, {{ block.settings.title_color_1 }}, {{ block.settings.title_color_2 }});
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
  }

  .wedding-diaries-subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.subtitle_size }}px;
    line-height: 1.6;
    margin-bottom: 40px;
    max-width: 600px;
    margin-left: auto;
    margin-right: auto;
    opacity: 0.9;
  }

  .wedding-diaries-cta-{{ ai_gen_id }} {
    display: inline-flex;
    align-items: center;
    gap: 12px;
    padding: 16px 32px;
    background: linear-gradient(135deg, {{ block.settings.button_bg_color_1 }}, {{ block.settings.button_bg_color_2 }});
    color: {{ block.settings.button_text_color }};
    text-decoration: none;
    border-radius: 50px;
    font-weight: 600;
    transition: all 0.3s ease;
    box-shadow: 0 8px 32px rgba(198, 59, 51, 0.3);
  }

  .wedding-diaries-cta-{{ ai_gen_id }}:hover {
    transform: translateY(-2px);
    box-shadow: 0 12px 40px rgba(198, 59, 51, 0.4);
    background: linear-gradient(135deg, {{ block.settings.button_hover_color_1 }}, {{ block.settings.button_hover_color_2 }});
  }

  .wedding-diaries-floating-elements-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    pointer-events: none;
    z-index: 1;
  }

  .wedding-diaries-heart-{{ ai_gen_id }} {
    position: absolute;
    color: rgba(198, 59, 51, 0.3);
    animation: wedding-diaries-float-{{ ai_gen_id }} 6s ease-in-out infinite;
  }

  .wedding-diaries-ring-{{ ai_gen_id }} {
    position: absolute;
    color: rgba(198, 59, 51, 0.2);
    animation: wedding-diaries-rotate-{{ ai_gen_id }} 8s linear infinite;
  }

  .wedding-diaries-sparkle-{{ ai_gen_id }} {
    position: absolute;
    color: rgba(198, 59, 51, 0.4);
    animation: wedding-diaries-twinkle-{{ ai_gen_id }} 3s ease-in-out infinite;
  }

  .wedding-diaries-features-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 32px;
    margin-top: 80px;
  }

  .wedding-diaries-feature-{{ ai_gen_id }} {
    background: rgba(255, 255, 255, 0.1);
    backdrop-filter: blur(10px);
    border: 1px solid rgba(198, 59, 51, 0.2);
    border-radius: 20px;
    padding: 32px;
    text-align: center;
    transition: all 0.3s ease;
  }

  .wedding-diaries-feature-{{ ai_gen_id }}:hover {
    transform: translateY(-8px);
    box-shadow: 0 20px 60px rgba(198, 59, 51, 0.2);
    border-color: rgba(198, 59, 51, 0.4);
  }

  .wedding-diaries-feature-icon-{{ ai_gen_id }} {
    width: 60px;
    height: 60px;
    margin: 0 auto 20px;
    background: linear-gradient(45deg, {{ block.settings.icon_color_1 }}, {{ block.settings.icon_color_2 }});
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    font-size: 24px;
    box-shadow: 0 8px 24px rgba(198, 59, 51, 0.3);
  }

  .wedding-diaries-feature-title-{{ ai_gen_id }} {
    font-size:20px;
    font-weight: 600;
    margin-bottom: 12px;
    color: {{ block.settings.text_color }};
  }

  .wedding-diaries-feature-text-{{ ai_gen_id }} {
    font-size: 16px;
    line-height: 1.5;
    color: {{ block.settings.text_color }};
    opacity: 0.8;
  }

  @keyframes wedding-diaries-float-{{ ai_gen_id }} {
    0%, 100% { transform: translateY(0px) rotate(0deg); }
    50% { transform: translateY(-20px) rotate(10deg); }
  }

  @keyframes wedding-diaries-rotate-{{ ai_gen_id }} {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(360deg); }
  }

  @keyframes wedding-diaries-twinkle-{{ ai_gen_id }} {
    0%, 100% { opacity: 0.2; transform: scale(1); }
    50% { opacity: 0.8; transform: scale(1.2); }
  }

  @media (max-width: 768px) {
    .wedding-diaries-section-{{ ai_gen_id }} {
      padding: 60px 16px;
      min-height: 80vh;
    }

    .wedding-diaries-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.7 }}px;
    }

    .wedding-diaries-subtitle-{{ ai_gen_id }} {
      font-size: {{ block.settings.subtitle_size | times: 0.9 }}px;
    }

    .wedding-diaries-features-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 24px;
      margin-top: 60px;
    }

    .wedding-diaries-feature-{{ ai_gen_id }} {
      padding: 24px;
    }
  }
{% endstyle %}

<wedding-diaries-{{ ai_gen_id }}
  class="wedding-diaries-section-{{ ai_gen_id }}"
  {{ block.shopify_attributes }}
>
  <div class="wedding-diaries-floating-elements-{{ ai_gen_id }}">
    {% for i in (1..12) %}
      {% assign element_type = forloop.index | modulo: 3 %}
      {% assign top = forloop.index | times: 8 | modulo: 80 %}
      {% assign left = forloop.index | times: 7 | modulo: 90 %}
      {% assign delay = forloop.index | times: 0.5 %}
      {% case element_type %}
        {% when 0 %}
          <div 
            class="wedding-diaries-heart-{{ ai_gen_id }}"
            style="top: {{ top }}%; left: {{ left }}%; animation-delay: {{ delay }}s;"
          >
            <svg width="24" height="24" viewBox="0 0 24 24" fill="currentColor">
              <path d="M12 21.35l-1.45-1.32C5.4 15.36 2 12.28 2 8.5 2 5.42 4.42 3 7.5 3c1.74 0 3.41.81 4.5 2.09C13.09 3.81 14.76 3 16.5 3 19.58 3 22 5.42 22 8.5c0 3.78-3.4 6.86-8.55 11.54L12 21.35z"/></svg>
          </div>
        {% when 1 %}
          <div 
            class="wedding-diaries-ring-{{ ai_gen_id }}"
            style="top: {{ top }}%; left: {{ left }}%; animation-delay: {{ delay }}s;"
          >
            <svg width="20" height="20" viewBox="0 0 24 24" fill="currentColor">
              <circle cx="12" cy="12" r="8" fill="none" stroke="currentColor" stroke-width="2"/>
              <circle cx="12" cy="8" r="2" fill="currentColor"/>
            </svg>
          </div>
        {% else %}
          <div 
            class="wedding-diaries-sparkle-{{ ai_gen_id }}"
            style="top: {{ top }}%; left: {{ left }}%; animation-delay: {{ delay }}s;"
          >
            <svg width="16" height="16" viewBox="0 0 24 24" fill="currentColor">
              <path d="M12 0l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-3.01L12 0z"/>
            </svg>
          </div>
      {% endcase %}
    {% endfor %}
  </div>

  <div class="wedding-diaries-container-{{ ai_gen_id }}">
    <div class="wedding-diaries-content-{{ ai_gen_id }}">
      {% if block.settings.badge_text != blank %}
        <div class="wedding-diaries-badge-{{ ai_gen_id }}">
          {{ block.settings.badge_text }}
        </div>
      {% endif %}

      {% if block.settings.title != blank %}
        <h1 class="wedding-diaries-title-{{ ai_gen_id }}">
          {{ block.settings.title }}
        </h1>
      {% endif %}

      {% if block.settings.subtitle != blank %}
        <p class="wedding-diaries-subtitle-{{ ai_gen_id }}">
          {{ block.settings.subtitle }}
        </p>
      {% endif %}

      {% if block.settings.cta_text != blank and block.settings.cta_url != blank %}
        <a href="{{ block.settings.cta_url }}" class="wedding-diaries-cta-{{ ai_gen_id }}">
          {{ block.settings.cta_text }}
          <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <path d="M5 12h14M12 5l7 7-7 7"/>
          </svg>
        </a>
      {% endif %}
    </div>

    {% if block.settings.show_features %}
      <div class="wedding-diaries-features-{{ ai_gen_id }}">
        {% for i in (1..3) %}
          {% liquid
            assign feature_title_key = 'feature_' | append: i | append: '_title'
            assign feature_text_key = 'feature_' | append: i | append: '_text'
            assign feature_title = block.settings[feature_title_key]
            assign feature_text = block.settings[feature_text_key]
          %}
          {% if feature_title != blank %}
            <div class="wedding-diaries-feature-{{ ai_gen_id }}">
              <div class="wedding-diaries-feature-icon-{{ ai_gen_id }}">
                {% case i %}
                  {% when 1 %}
                    <svg width="24" height="24" viewBox="0 0 24 24" fill="currentColor">
                      <path d="M12 21.35l-1.45-1.32C5.4 15.36 2 12.28 2 8.5 2 5.42 4.42 3 7.5 3c1.74 0 3.41.81 4.5 2.09C13.09 3.81 14.76 3 16.5 3 19.58 3 22 5.42 22 8.5c0 3.78-3.4 6.86-8.55 11.54L12 21.35z"/>
                    </svg>
                  {% when 2 %}
                    <svg width="24" height="24" viewBox="0 0 24 24" fill="currentColor">
                      <path d="M9 11H7l3-7 3 7h-2l1 7-3-7z"/>
                    </svg>
                  {% when 3 %}
                    <svg width="24" height="24" viewBox="0 0 24 24" fill="currentColor">
                      <path d="M12 0l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-3.01L12 0z"/>
                    </svg>
                {% endcase %}
              </div>
              <h3 class="wedding-diaries-feature-title-{{ ai_gen_id }}">{{ feature_title }}</h3>
              {% if feature_text != blank %}
                <p class="wedding-diaries-feature-text-{{ ai_gen_id }}">{{ feature_text }}</p>
              {% endif %}
            </div>
          {% endif %}
        {% endfor %}
      </div>
    {% endif %}
  </div>
</wedding-diaries-{{ ai_gen_id }}><script>
  (function() {
    class WeddingDiaries{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.initParallaxEffect();
        this.initIntersectionObserver();}

      initParallaxEffect() {
        const floatingElements = this.querySelectorAll('.wedding-diaries-heart-{{ ai_gen_id }}, .wedding-diaries-ring-{{ ai_gen_id }}, .wedding-diaries-sparkle-{{ ai_gen_id }}');
        
        window.addEventListener('mousemove', (e) => {
          const mouseX = e.clientX / window.innerWidth;
          const mouseY = e.clientY / window.innerHeight;
          
          floatingElements.forEach((element, index) => {
            const speed = (index % 3+ 1) * 0.5;
            const x = (mouseX - 0.5) * speed * 20;
            const y = (mouseY - 0.5) * speed * 20;
            
            element.style.transform = `translate(${x}px, ${y}px)`;});
        });
      }

      initIntersectionObserver() {
        const features = this.querySelectorAll('.wedding-diaries-feature-{{ ai_gen_id }}');
        
        const observer = new IntersectionObserver((entries) => {
          entries.forEach((entry, index) => {
            if (entry.isIntersecting) {
              setTimeout(() => {
                entry.target.style.opacity = '1';
                entry.target.style.transform = 'translateY(0)';
              }, index * 200);
            }
          });
        }, {
          threshold: 0.1
        });

        features.forEach((feature) => {
          feature.style.opacity = '0';
          feature.style.transform = 'translateY(30px)';
          feature.style.transition = 'all 0.6s ease';
          observer.observe(feature);
        });
      }
    }

    customElements.define('wedding-diaries-{{ ai_gen_id }}', WeddingDiaries{{ ai_gen_id }});
  })();
</script>

